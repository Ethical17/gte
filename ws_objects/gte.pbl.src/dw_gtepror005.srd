				 sum(decode(sign(tad.task_date - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(tad.task_date - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,(nvl(tad.task_tmatodayty,0) + nvl(tad.task_tfatodayty,0) + (0.5 * nvl(tad.task_tmadtodayty,0)) + (0.5 * nvl(tad.task_tfadtodayty,0)) + (0.5 * nvl(tad.task_tmctodayty,0)) + (0.5 * nvl(tad.task_tfctodayty,0)) + nvl(tad.task_omatodayty,0) + nvl(tad.task_ofatodayty,0) + (0.5 * nvl(tad.task_omadtodayty,0)) + (0.5 * nvl(tad.task_ofadtodayty,0)) + (0.5 * nvl(tad.task_omctodayty,0)) + (0.5 * nvl(tad.task_ofctodayty,0)))))) ltoday, 
