		GROUP BY DECODE (esub.eachead_id,'SLEG0015', '1.PLUCKING','SLEG0016', '3.FACTORY ACTIVITIES','SLEG0019', '3.FACTORY ACTIVITIES','SLEG0018','3.FACTORY ACTIVITIES','SLEG0017', '3.FACTORY ACTIVITIES','SLEG0020', '3.FACTORY ACTIVITIES','SLEG0010', '2.PLANTATION ACTIVITIES','SLEG0012', '2.PLANTATION ACTIVITIES','SLEG0013', '2.PLANTATION ACTIVITIES','SLEG0014', '2.PLANTATION ACTIVITIES','SLEG0021', '2.PLANTATION ACTIVITIES','SLEG0022', '2.PLANTATION ACTIVITIES','SLEG0023', '2.PLANTATION ACTIVITIES','SLEG0024', '2.PLANTATION ACTIVITIES','4.NON-PRODUCTIVE MANDAYS')
