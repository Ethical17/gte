				 	sum(decode(sign(glfp.glfp_pluckingdate - to_date(:ra_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(glfp.glfp_pluckingdate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(gwtm.gwtm_teamade,0)))) ctodate,
					sum(decode(sign(glfp.glfp_pluckingdate - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(glfp.glfp_pluckingdate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(gwtm.gwtm_teamade,0)))) ltoday, 
					sum(decode(sign(glfp.glfp_pluckingdate - to_date(:ra_ly_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(glfp.glfp_pluckingdate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(gwtm.gwtm_teamade,0)))) ltodate
		FROM fb_glforproduction glfp, fb_supplier sup,fb_gardenwiseteamade gwtm   
		WHERE glfp.sup_id = sup.sup_id AND glfp.glfp_pk = gwtm.glfp_pk AND (gwtm.gwtm_type in ('N' ,'O','E')) and NVL (gwtm.gwtm_teamade, 0) > 0    and 
				   (glfp.glfp_pluckingdate between to_date(:ra_ysdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') or glfp.glfp_pluckingdate between to_date(:ra_ly_ysdt,'dd/mm/yyyy') and to_date(:ra_ly_todt,'dd/mm/yyyy') ) 
    		GROUP BY '08',DECODE (TRIM (gwtm.gwtm_type), 'O', 'Own Crop - Other Factory',DECODE (TRIM (sup.sup_type), 'OWN', 'Own Crop - OWN Factory', 'Other Crop - OWN Factory'))
		UNION ALL    
		SELECT '09', 'OUT-TURN PER',mandays, 0 ctoday, 0 ctodate,0 ltoday,0 ltodate FROM (select ' PLUCKING MANDAYS' mandays from dual union select 'MANUFACTURING MANDAYS'  from dual)
		UNION ALL    
		SELECT   '10', 'FUEL CONSUMPTION', DECODE (ct.ct_id, 'CT00001', 'ENERGY', 'FUEL'), 
				 	sum(decode(sign(ddp.ddp_pluckingdate - to_date(:ra_frdt,'dd/mm/yyyy')),-1,0,decode(sign(ddp.ddp_pluckingdate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(ddc.ddc_unitconsumed,0)))) ctoday, 
				 	sum(decode(sign(ddp.ddp_pluckingdate - to_date(:ra_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(ddp.ddp_pluckingdate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(ddc.ddc_unitconsumed,0)))) ctodate,
					sum(decode(sign(ddp.ddp_pluckingdate - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(ddp.ddp_pluckingdate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(ddc.ddc_unitconsumed,0)))) ltoday, 
					sum(decode(sign(ddp.ddp_pluckingdate - to_date(:ra_ly_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(ddp.ddp_pluckingdate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(ddc.ddc_unitconsumed,0)))) ltodate
		FROM fb_dailydryerproduct ddp, fb_dailydryerconsumption ddc, fb_consumptiontype ct, fb_machine mac  
		WHERE ddp.ddp_pk = ddc.ddp_pk AND ddp.dryer_id = mac.machine_id AND ddc.ct_id = ct.ct_id and NVL (ddc.ddc_unitconsumed, 0) <> 0    and
				(ddp.ddp_pluckingdate between to_date(:ra_ysdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') or ddp.ddp_pluckingdate between to_date(:ra_ly_ysdt,'dd/mm/yyyy') and to_date(:ra_ly_todt,'dd/mm/yyyy') ) 
		GROUP BY DECODE (ct.ct_id, 'CT00001', 'ENERGY', 'FUEL')
		UNION ALL   
		SELECT   '10.5', 'SORTED TEA MADE', 'SORTED TEA MADE', 
					sum(decode(sign(dtmp.dtmp_sortdate - to_date(:ra_frdt,'dd/mm/yyyy')),-1,0,decode(sign(dtmp.dtmp_sortdate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(dtmp.dtmp_sortquantodayty,0)))) ctoday, 
					sum(decode(sign(dtmp.dtmp_sortdate - to_date(:ra_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(dtmp.dtmp_sortdate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(dtmp.dtmp_sortquantodayty,0)))) ctodate,
					sum(decode(sign(dtmp.dtmp_sortdate - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(dtmp.dtmp_sortdate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(dtmp.dtmp_sortquantodayty,0)))) ltoday, 
					sum(decode(sign(dtmp.dtmp_sortdate - to_date(:ra_ly_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(dtmp.dtmp_sortdate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(dtmp.dtmp_sortquantodayty,0)))) ltodate
		FROM fb_dailysortedteamadeproduct dtmp,fb_teamadeproduct tmp,fb_teamadeproductcategory tpc 
		WHERE tpc.tpc_id = tmp.tpc_id AND dtmp.tmp_id = tmp.tmp_id and 
		(dtmp.dtmp_sortdate between to_date(:ra_ysdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') or dtmp.dtmp_sortdate between to_date(:ra_ly_ysdt,'dd/mm/yyyy') and to_date(:ra_ly_todt,'dd/mm/yyyy') )
		UNION ALL    
		SELECT   '11', 'PACKED TEA MADE', 'PACKED TEA MADE', 
				 	sum(decode(sign(dtp.dtp_date - to_date(:ra_frdt,'dd/mm/yyyy')),-1,0,decode(sign(dtp.dtp_date - to_date(:ra_todt,'dd/mm/yyyy')),1,0,(( (nvl(dtpd.dtpd_srnoend,0) - nvl(dtpd.dtpd_srnostart,0)) + 1)  * nvl(dtpd.dtpd_indwt,0))))) ctoday, 
				 	sum(decode(sign(dtp.dtp_date - to_date(:ra_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(dtp.dtp_date - to_date(:ra_todt,'dd/mm/yyyy')),1,0,(( (nvl(dtpd.dtpd_srnoend,0) - nvl(dtpd.dtpd_srnostart,0)) + 1)  * nvl(dtpd.dtpd_indwt,0))))) ctodate,
					sum(decode(sign(dtp.dtp_date - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(dtp.dtp_date - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,(( (nvl(dtpd.dtpd_srnoend,0) - nvl(dtpd.dtpd_srnostart,0)) + 1)  * nvl(dtpd.dtpd_indwt,0))))) ltoday, 
					sum(decode(sign(dtp.dtp_date - to_date(:ra_ly_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(dtp.dtp_date - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,(( (nvl(dtpd.dtpd_srnoend,0) - nvl(dtpd.dtpd_srnostart,0)) + 1)  * nvl(dtpd.dtpd_indwt,0))))) ltodate
		FROM fb_dailyteapacked dtp, fb_dtpdetails dtpd, fb_teamadeproductcategory tpc, fb_teamadeproduct tmp 
		WHERE tpc.tpc_id = tmp.tpc_id  AND dtpd.tmp_id = tmp.tmp_id AND dtp.dtp_id = dtpd.dtp_id and 
				(dtp.dtp_date between to_date(:ra_ysdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') or dtp.dtp_date between to_date(:ra_ly_ysdt,'dd/mm/yyyy') and to_date(:ra_ly_todt,'dd/mm/yyyy') ) 
		UNION ALL    
		SELECT   '12', 'DESPATCH TEA MADE', 'DESPATCH TEA MADE', 
				 	sum(decode(sign(si.si_date - to_date(:ra_frdt,'dd/mm/yyyy')),-1,0,decode(sign(si.si_date - to_date(:ra_todt,'dd/mm/yyyy')),1,0,(((nvl(SID.sid_srnoend,0) - nvl(SID.sid_srnostart,0)) + 1) * nvl(dtpd.dtpd_indwt,0))))) ctoday, 
				 	sum(decode(sign(si.si_date - to_date(:ra_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(si.si_date - to_date(:ra_todt,'dd/mm/yyyy')),1,0,(((nvl(SID.sid_srnoend,0) - nvl(SID.sid_srnostart,0)) + 1) * nvl(dtpd.dtpd_indwt,0))))) ctodate,
					sum(decode(sign(si.si_date - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(si.si_date - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,(((nvl(SID.sid_srnoend,0) - nvl(SID.sid_srnostart,0)) + 1) * nvl(dtpd.dtpd_indwt,0))))) ltoday, 
					sum(decode(sign(si.si_date - to_date(:ra_ly_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(si.si_date - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,(((nvl(SID.sid_srnoend,0) - nvl(SID.sid_srnostart,0)) + 1) * nvl(dtpd.dtpd_indwt,0))))) ltodate
		  FROM fb_saleinvoice si, fb_sidetails SID, fb_dtpdetails dtpd, fb_teamadeproductcategory tpc,fb_teamadeproduct tmp
		WHERE tpc.tpc_id = tmp.tpc_id AND dtpd.tmp_id = tmp.tmp_id AND SID.dtpd_id = dtpd.dtpd_id AND si.si_id = SID.si_id AND si.si_active = '1' and (((nvl(SID.sid_srnoend,0) - nvl(SID.sid_srnostart,0)) + 1) * nvl(dtpd.dtpd_indwt,0)) > 0 and
				(si.si_date between to_date(:ra_ysdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') or si.si_date between to_date(:ra_ly_ysdt,'dd/mm/yyyy') and to_date(:ra_ly_todt,'dd/mm/yyyy') ) 
		UNION ALL
	    SELECT   '13', 'ALL', 'UNSORTED - KG', 
				 	sum(decode(sign(pludate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ctoday, 
				 	sum(decode(sign(pludate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ctodate,
					sum(decode(sign(pludate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ltoday, 
					sum(decode(sign(pludate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ltodate
		FROM (SELECT   ddu.tpc_id tpc_id, ddp.ddp_pluckingdate pludate, NVL (SUM (ddu.ddu_quantity), 0) qty  
					 FROM fb_dailydryerproduct ddp, fb_dailydryerunsorted ddu
					WHERE ddp.ddp_pk = ddu.ddp_pk AND ddp.ddp_type <> 'O' 
					 GROUP BY ddu.tpc_id, ddp.ddp_pluckingdate UNION ALL  
					SELECT   tmp.tpc_id tpc_id, dtmp_sortdate pludate,-NVL (SUM (dtmp.dtmp_sortquantodayty), 0) qty 
					  FROM fb_dailysortedteamadeproduct dtmp,fb_teamadeproduct tmp WHERE dtmp.tmp_id = tmp.tmp_id 
					GROUP BY (tmp.tpc_id, dtmp.dtmp_sortdate)) d1, fb_teamadeproductcategory tpc 
		 WHERE (tpc.tpc_id = d1.tpc_id) and  (pludate <=  to_date(:ra_todt,'dd/mm/yyyy') ) 
		UNION ALL    
		SELECT   '14', 'ALL',  'SORTED BUT NOT PACKED  - KG',
				 	sum(decode(sign(pludate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ctoday, 
				 	sum(decode(sign(pludate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ctodate,
					sum(decode(sign(pludate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ltoday, 
					sum(decode(sign(pludate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ltodate
		FROM (SELECT tmp.tpc_id tpc_id, dtmp_sortdate pludate, NVL (SUM (dtmp.dtmp_sortquantodayty), 0) qty 
		      	  FROM fb_dailysortedteamadeproduct dtmp,fb_teamadeproduct tmp
			  	 WHERE dtmp.tmp_id = tmp.tmp_id 
			  	GROUP BY (tmp.tpc_id, dtmp.dtmp_sortdate) UNION ALL 
				SELECT   tmp.tpc_id tpc_id, dtp.dtp_date pludate, -NVL (SUM (  (dtpd.dtpd_srnoend - dtpd.dtpd_srnostart + 1 ) * dtpd.dtpd_indwt),0 ) qty
				FROM fb_dailyteapacked dtp, fb_dtpdetails dtpd, fb_teamadeproduct tmp 
				WHERE dtp.dtp_id = dtpd.dtp_id AND tmp.tmp_id = dtpd.tmp_id 
				GROUP BY (tmp.tpc_id, dtp.dtp_date)) d1, fb_teamadeproductcategory tpc  
		WHERE (tpc.tpc_id = d1.tpc_id)    and (pludate <= to_date(:ra_todt,'dd/mm/yyyy')) 
		UNION ALL 
		SELECT   '15', 'ALL','PACKED BUT AWAITING DESPATCH - KG', 
				 	sum(decode(sign(pludate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ctoday, 
				 	sum(decode(sign(pludate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ctodate,
					sum(decode(sign(pludate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ltoday, 
					sum(decode(sign(pludate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ltodate
		FROM (SELECT   tmp.tpc_id tpc_id, dtp.dtp_date pludate, NVL (SUM (  (dtpd.dtpd_srnoend - dtpd.dtpd_srnostart + 1) * dtpd.dtpd_indwt), 0) qty
				  FROM fb_dailyteapacked dtp, fb_dtpdetails dtpd, fb_teamadeproduct tmp 
				  WHERE dtp.dtp_id = dtpd.dtp_id AND tmp.tmp_id = dtpd.tmp_id 
				  GROUP BY (tmp.tpc_id, dtp.dtp_date)  UNION ALL   
				  SELECT tmp.tpc_id tpc_id, si.si_date pludate, -NVL (SUM (  (SID.sid_srnoend - SID.sid_srnostart + 1) * dtpd.dtpd_indwt), 0) qty
				  FROM fb_dtpdetails dtpd LEFT OUTER JOIN fb_sidetails SID ON dtpd.dtpd_id = SID.dtpd_id LEFT OUTER JOIN fb_saleinvoice si ON  SID.si_id = si.si_id , fb_teamadeproduct tmp 
				  WHERE dtpd.tmp_id = tmp.tmp_id AND si.si_active = '1' 
				  GROUP BY (tmp.tpc_id, si.si_date)) d1,fb_teamadeproductcategory tpc 
		WHERE (tpc.tpc_id = d1.tpc_id)  and (pludate <= to_date(:ra_todt,'dd/mm/yyyy') ) 
		UNION ALL    
		SELECT   '16', 'ALL', 'FACTORY TOTAL STOCK - KG',
				 	sum(decode(sign(pludate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ctoday, 
				 	sum(decode(sign(pludate - to_date(:ra_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ctodate,
					sum(decode(sign(pludate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ltoday, 
					sum(decode(sign(pludate - to_date(:ra_ly_todt,'dd/mm/yyyy')),1,0,nvl(qty,0))) ltodate
		FROM (SELECT   ddu.tpc_id tpc_id, ddp.ddp_pluckingdate pludate, NVL (SUM (ddu.ddu_quantity), 0) qty 
				  FROM fb_dailydryerproduct ddp, fb_dailydryerunsorted ddu 
				  WHERE ddp.ddp_pk = ddu.ddp_pk AND ddp.ddp_type <> 'O' 
				  GROUP BY (ddu.tpc_id, ddp.ddp_pluckingdate) 
				  UNION ALL 
				  SELECT tmp.tpc_id tpc_id, si.si_date pludate, -NVL (SUM (  (SID.sid_srnoend - SID.sid_srnostart + 1) * dtpd.dtpd_indwt),0) qty 
				  FROM fb_dtpdetails dtpd LEFT OUTER JOIN fb_sidetails SID  ON dtpd.dtpd_id = SID.dtpd_id LEFT OUTER JOIN fb_saleinvoice si ON SID.si_id = si.si_id , fb_teamadeproduct tmp 
				  WHERE dtpd.tmp_id = tmp.tmp_id AND si.si_active = '1' 
				  GROUP BY (tmp.tpc_id, si.si_date)) d1, fb_teamadeproductcategory tpc       
		WHERE (tpc.tpc_id = d1.tpc_id) and (pludate <= to_date(:ra_todt,'dd/mm/yyyy') ) 
			) a" arguments=(("ra_frdt", string),("ra_todt", string),("ra_ysdt", string),("ra_ly_frdt", string),("ra_ly_todt", string),("ra_ly_ysdt", string),("ra_tot_area", number),("ra_mf_mdtday", number),("ra_mf_mdtdate", number),("ra_mf_mdlytday", number),("ra_mf_mdlytdate", number))  sort="particulars A gttype A recname A " )
