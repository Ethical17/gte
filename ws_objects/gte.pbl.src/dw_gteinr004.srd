 retrieve="select c.SPC_ID, c.SPC_NAME,
       sum(decode(sign(trunc(ds_date) - to_date(:ra_fr_dt,'dd/mm/yyyy')),-1,decode(ds_rec_ind,'I',-1,1),0) * ds_QTY) opn,
       sum(decode(sign(trunc(ds_date) - to_date(:ra_fr_dt,'dd/mm/yyyy')),-1,decode(ds_rec_ind,'I',-1,1),0) * nvl(ds_Value,0)) opnvalue,
       sum(decode(sign(trunc(ds_date) - to_date(:ra_fr_dt,'dd/mm/yyyy')),-1,0,decode(ds_rec_ind,'I',decode(DS_REF_TYPE,'Store Adjustment',decode(sign(nvl(ds_Value,0)),-1,0,1),1),0) * ds_QTY)) isu,
       sum(decode(sign(trunc(ds_date) - to_date(:ra_fr_dt,'dd/mm/yyyy')),-1,0,decode(ds_rec_ind,'I',decode(DS_REF_TYPE,'Store Adjustment',decode(sign(nvl(ds_Value,0)),-1,0,1),1),0) * nvl(ds_Value,0))) isuvalue,
       sum(decode(sign(trunc(ds_date) - to_date(:ra_fr_dt,'dd/mm/yyyy')),-1,0,decode(ds_rec_ind,'R',1,decode(DS_REF_TYPE,'Store Adjustment',decode(sign(nvl(ds_Value,0)),-1,-1,0),0))) * ds_QTY) recv,
       sum(decode(sign(trunc(ds_date) - to_date(:ra_fr_dt,'dd/mm/yyyy')),-1,0,decode(ds_rec_ind,'R',1,decode(DS_REF_TYPE,'Store Adjustment',decode(sign(nvl(ds_Value,0)),-1,-1,0),0))) * nvl(ds_Value,0)) recvalue,
       sum(decode(ds_rec_ind,'I',-1,1)*ds_QTY) clos,
       sum(decode(ds_rec_ind,'I',-1,1)* nvl(ds_Value,0)) closvalue
  from fb_daily_stock a, FB_STOREPRODUCTCATEGORY c, 
        (select * from FB_STOREPRODUCT where (to_date(:ra_fr_dt,'dd/mm/yyyy') < to_date('01/01/2015','dd/mm/yyyy') or  ((nvl(sp_nppc_ind,'N') = 'N' and to_date(:ra_fr_dt,'dd/mm/yyyy') >= to_date('01/01/2015','dd/mm/yyyy')) or 
                                                                   (:ra_user = 'ADMIN' and :ra_nppc = 'Y')))) b
where decode(sign(trunc(ds_date) - to_date(:ra_fr_dt,'dd/mm/yyyy')),-1, to_date(:ra_fr_dt,'dd/mm/yyyy'),trunc(ds_date)) <= TO_DATE (:ra_to_dt,'DD/MM/YYYY') and 
       DS_SECTION_ID= :ra_store_id and a.DS_ITEM_CD=b.SP_ID AND b.SPC_ID=c.SPC_ID
group by c.SPC_ID, c.SPC_NAME
ORDER BY c.SPC_ID" arguments=(("ra_fr_dt", string),("ra_to_dt", string),("ra_store_id", string),("ra_user", string),("ra_nppc", string)) )
