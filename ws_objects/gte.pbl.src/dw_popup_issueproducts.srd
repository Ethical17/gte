 retrieve="select SP_ID,SP_NAME,SP_UNIT,unit_prise, (iss_qty ) PRIS_ISSUEQUANTITY,EACSUBHEAD_ID ,SPC_ID,PRIS_ID,price_id, price_name
from 
(select b.SP_ID,decode(nvl(a.SSP_ID,'x'),'x',SP_NAME,c.SSP_NAME)SP_NAME,SP_UNIT, decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY) unit_prise,sum(PRIS_ISSUEQUANTITY) iss_qty ,EACSUBHEAD_ID ,SPC_ID,PRIS_ID,SPC_ID||'-'||round((decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY)),2) price_id,
        SP_NAME||'('||round((decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY)),2)||')' price_name
 from fb_storeproduct a,fb_productissuedetails b,fb_storeproduct_std c
where  b.SP_ID=a.SP_ID(+) and a.SSP_ID=c.ssp_id(+)  
group by b.SP_ID,decode(nvl(a.SSP_ID,'x'),'x',SP_NAME,c.SSP_NAME),SP_UNIT,EACSUBHEAD_ID ,SPC_ID,PRIS_ID, decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY), SPC_ID||'-'||round((decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY)),2),
        SP_NAME||'('||round((decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY)),2)||')')    
union
select aa.SP_ID,SP_NAME,SP_UNIT,unit_prise, (iss_qty - ret_qty) PRIS_ISSUEQUANTITY,EACSUBHEAD_ID ,SPC_ID,PRIS_ID,price_id, price_name
from 
(select b.SP_ID,decode(nvl(a.SSP_ID,'x'),'x',SP_NAME,c.SSP_NAME)SP_NAME,SP_UNIT, decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY) unit_prise,sum(PRIS_ISSUEQUANTITY) iss_qty ,EACSUBHEAD_ID ,SPC_ID,PRIS_ID,SPC_ID||'-'||round((decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY)),2) price_id,
        SP_NAME||'('||round((decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY)),2)||')' price_name
 from fb_storeproduct a,fb_productissuedetails b,fb_storeproduct_std c
where  b.SP_ID=a.SP_ID(+) and a.SSP_ID=c.ssp_id(+)  
group by b.SP_ID,decode(nvl(a.SSP_ID,'x'),'x',SP_NAME,c.SSP_NAME),SP_UNIT,EACSUBHEAD_ID ,SPC_ID,PRIS_ID, decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY), SPC_ID||'-'||round((decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY)),2),
        SP_NAME||'('||round((decode(nvl(PRIS_ISSUEQUANTITY,0),0,0,PRIS_VALUE/PRIS_ISSUEQUANTITY)),2)||')') aa,
(select PRET_ISSUE_ID, SP_ID, nvl(sum(PRET_RETURNQUANTITY),0) ret_qty from FB_PRODUCTRETURN a,FB_PRODUCTRETURNDETAILS b 
	where a.PRET_ID=b.PRET_ID group by PRET_ISSUE_ID, SP_ID) d
where aa.PRIS_ID = PRET_ISSUE_ID and aa.sp_id = d.sp_id  "  sort="pris_id D sp_name A " )
