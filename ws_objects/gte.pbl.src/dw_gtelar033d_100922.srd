table(column=(type=number updatewhereclause=yes name=pfno dbname="pfno" )
 column=(type=char(50) updatewhereclause=yes name=empname dbname="empname" )
 column=(type=number updatewhereclause=yes name=gross_wages dbname="gross_wages" )
 column=(type=number updatewhereclause=yes name=epf_wages dbname="epf_wages" )
 column=(type=number updatewhereclause=yes name=eps_wage dbname="eps_wage" )
 column=(type=number updatewhereclause=yes name=edli_wage dbname="edli_wage" )
 column=(type=number updatewhereclause=yes name=epf_contribution_remit dbname="epf_contribution_remit" )
 column=(type=number updatewhereclause=yes name=eps_contribution_remit dbname="eps_contribution_remit" )
 column=(type=number updatewhereclause=yes name=diff_epf_eps_remit dbname="diff_epf_eps_remit" )
 column=(type=number updatewhereclause=yes name=ncp_days dbname="ncp_days" )
 column=(type=number updatewhereclause=yes name=adv dbname="adv" )
 retrieve="SELECT pfno , empname, 
       round(sum(nvl(epf_wages,0)),0) gross_wages,
       round(sum(nvl(epf_wages,0)),0) epf_wages,
       round(sum(nvl(eps_wage,0)),0) eps_wage,
       round(sum(nvl(eps_wage,0)),0) edli_wage,
       sum(nvl(epf_contribution_remit,0)) epf_contribution_remit,
       sum(nvl(eps_contribution_remit,0)) eps_contribution_remit,
       sum(nvl(diff_epf_eps_remit,0)) diff_epf_eps_remit,
       max(nvl(ncp_days,0)) ncp_days, 0 adv
from    (SELECT to_number(a.EMP_UAN_NO) pfno , upper(a.EMP_NAME) empname, 
                 round(sum(b.LABOUR_WAGES + b.LABOUR_MATERNITYEARN + b.LABOUR_SICKEARN + b.LABOUR_ELPEARN  + b.LABOUR_FOODCONC + nvl(b.LABOUR_ATTN_INC,0) + nvl(LABOUR_OTHEREARN,0)  - nvl(LABOUR_PENALTY,0) ),0) epf_wages,
                 round(sum(decode(EMP_PFDEDCODE,'1',0,decode(sign(1250 - nvl(LABOUR_FPF,0)),1,(b.LABOUR_WAGES + b.LABOUR_MATERNITYEARN + b.LABOUR_SICKEARN + b.LABOUR_ELPEARN +  b.LABOUR_FOODCONC + nvl(b.LABOUR_ATTN_INC,0) + nvl(LABOUR_OTHEREARN,0)  - nvl(LABOUR_PENALTY,0)),15000))),0) eps_wage,
                 sum((b.LABOUR_PF + b.LABOUR_FPF )) epf_contribution_due,
                sum((b.LABOUR_PF + b.LABOUR_FPF )) epf_contribution_remit,
                 sum(nvl(b.LABOUR_FPF,0) ) eps_contribution_due,
                 round(sum(decode(EMP_PFDEDCODE,'1',0,decode(sign(1250 - nvl(LABOUR_FPF,0)),1,nvl(b.LABOUR_FPF,0),1250 )))) eps_contribution_remit,
                 sum(nvl(b.LABOUR_PF,0) ) diff_epf_eps_due,sum(nvl(b.LABOUR_PF,0) ) diff_epf_eps_remit, max(nvl(absdays,0)) ncp_days
        FROM FB_EMPLOYEE a, FB_LABOURWEEKLYWAGES b,FB_LABOURWAGESWEEK c,fb_field d,
            (select labour_id, count(*) absdays from fb_labourdailyattendance 
             where lda_date between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and 
                       kamsub_id = (select kamsub_id from fb_kamsubhead where  nvl(KAMSUB_ACTIVE_IND,'N') = 'Y' and KAMSUB_NKAMTYPE = 'ABSENT')
             group by labour_id) e
      WHERE (a.EMP_ID=b.LABOUR_ID) AND (b.LWW_ID=c.LWW_ID) AND 
                LWW_STARTDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and
                LWW_ENDDATE between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') and
                (:ra_gsnm = 'MK' or (:ra_gsnm <> 'MK' and nvl(to_number(a.EMP_UAN_NO),0) > 0)) AND  LWW_PAIDFLAG = '1' and
                a.emp_id = e.labour_id(+) and a.field_id = d.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
    group by to_number(a.EMP_UAN_NO) , upper(a.EMP_NAME)
    union
    select to_number(d.EMP_UAN_NO) pfno , upper(d.EMP_NAME) empname,
        sum(nvl(LWW_EARN,0) + nvl(LWW_FOODCONC,0)) LWW_EARN,
        round(sum(decode(EMP_PFDEDCODE,'1',0,(nvl(LWW_EARN,0) + nvl(LWW_FOODCONC,0)))),0), 
        sum(nvl(LWW_PF,0) + nvl(LWW_FPF,0)), 
        sum(nvl(LWW_PF,0) + nvl(LWW_FPF,0)) ,
        sum(nvl(LWW_FPF,0)),
        sum(nvl(LWW_FPF,0)),
        sum(nvl(LWW_PF,0)),
        sum(nvl(LWW_PF,0)), 0
            from FB_LABLWW a, fb_lablwwperiod b, fb_lablwwseason c,FB_EMPLOYEE d,fb_field e
            where a.LLP_ID = b.LLP_ID and b.LLS_ID = c.LLS_ID and d.EMP_ID = a.LABOUR_ID and d.field_id = e.field_id and d.EMP_UAN_NO is not null and
                  LLS_PDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
    group by to_number(d.EMP_UAN_NO) , upper(d.EMP_NAME)
    union
    select to_number(c.EMP_UAN_NO) pfno , upper(c.EMP_NAME) empname,
        sum(nvl(LA_AMOUNT,0)) LWW_EARN,
        round(sum(decode(EMP_PFDEDCODE,'1',0,(nvl(LA_AMOUNT,0)))),0), 
        sum(nvl(LA_PF,0) + nvl(LA_FPF,0)), 
        sum(nvl(LA_PF,0) + nvl(LA_FPF,0)) ,
        sum(nvl(LA_FPF,0)),
        sum(nvl(LA_FPF,0)),
        sum(nvl(LA_PF,0)),
        sum(nvl(LA_PF,0)), sum(nvl(LA_DAYSWORKED,0))
            FROM fb_labourarrear a,fb_arrearperiod b,FB_EMPLOYEE c,fb_field e
            where a.ap_id = b.ap_id  and c.EMP_ID = a.LABOUR_ID and c.field_id = e.field_id  and c.EMP_PFNO is not null and
                  LA_VOU_DT between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
    group by to_number(c.EMP_UAN_NO) , upper(c.EMP_NAME))
group by pfno , empname
union all
SELECT to_number(a.EMP_UAN_NO) pfno , upper(a.EMP_NAME) empname, 
         round(nvl(EP_BASICAMOUNT,0) + nvl(EP_DAAMOUNT,0) + nvl(EP_VDAAMOUNT,0) + nvl(EP_CVRFORPFAMOUNT,0) + nvl(EP_ALLOWWPF,0),0) gross_wages,
         round(nvl(EP_BASICAMOUNT,0) + nvl(EP_DAAMOUNT,0) + nvl(EP_VDAAMOUNT,0) + nvl(EP_CVRFORPFAMOUNT,0) + nvl(EP_ALLOWWPF,0),0) epf_wages,
         decode(EMP_PFDEDCODE,'1',0,'2',decode(sign(1250 - nvl(EP_PENSIONFUNDDEDAMOUNT,0)),1,round(nvl(EP_BASICAMOUNT,0) + nvl(EP_DAAMOUNT,0) + nvl(EP_VDAAMOUNT,0) + nvl(EP_CVRFORPFAMOUNT,0) + nvl(EP_ALLOWWPF,0),0),15000)) eps_wage,
         decode(EMP_PFDEDCODE,'1',0,'2',decode(sign(1250 - nvl(EP_PENSIONFUNDDEDAMOUNT,0)),1,round(nvl(EP_BASICAMOUNT,0) + nvl(EP_DAAMOUNT,0) + nvl(EP_VDAAMOUNT,0) + nvl(EP_CVRFORPFAMOUNT,0) + nvl(EP_ALLOWWPF,0),0),15000)) edli_wage,         
        (nvl(EP_PENSIONFUNDDEDAMOUNT,0) + nvl(EP_PFCONTDEDAMOUNT,0)) epf_contribution_remit,
        decode(EMP_PFDEDCODE,'1',0,'2',decode(sign(1250 - nvl(EP_PENSIONFUNDDEDAMOUNT,0)),1,nvl(EP_PENSIONFUNDDEDAMOUNT,0),1250)) eps_contribution_remit,
        nvl(EP_PFCONTDEDAMOUNT,0) diff_epf_eps_remit, nvl(absdays,0) ncp_days,0
FROM FB_EMPLOYEE a,fb_emppayment b, fb_field d,
     (select EMP_ID, count(*) absdays from fb_empattendance where to_number(to_char(EATTEN_DATE,'YYYYMM')) = :ra_yrmn and EATTEN_STATUS = 'AB'
      group by EMP_ID) c
where a.EMP_ID = b.EMP_ID and a.emp_id = c.emp_id(+) and ((nvl(EP_YEAR,0) * 100) + nvl(EP_MONTH,0)) = :ra_yrmn and 
      a.field_id = d.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf" arguments=(("ra_frdt", string),("ra_todt", string),("ra_gsnm", string),("ra_yrmn", number),("ra_pf", string),("ra_gpf", string))  sort="pfno A " )
