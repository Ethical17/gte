 retrieve="select c.ADVICE_ID,b.ADVICE_DATE,c.SI_ID Invoice_no,c.SI_DATE invoice_dt,SI_CN consignment_no,
	   DTP_LOTNO lot_no,TMP_NAME tea_grade,SI_CNDATE,TRANSP_NAME,CUS_NAME,UNIT_NAME,CITY_NAME,UNIT_ADD,
		SI_CENTRALEXCISENO, SI_PRINT_DT,
       	sum((d.SID_SRNOEND-d.SID_SRNOSTART)+1) Bags,
	   	sum(((DTPD_SRNOEND-DTPD_SRNOSTART)+1)*(DTPD_INDWT)) totgross_wt   
 from fb_saleinvoice c,fb_sidetails d, fb_teamadeproduct e,
  	   (select distinct a.ADVICE_ID,ADVICE_DATE,a.CUST_ID, initcap(CUS_NAME) CUS_NAME,initcap(CITY_NAME) CITY_NAME
		from fb_advice a,fb_advicedetails d,fb_customer b, fb_city c 
		where a.ADVICE_ID = d.ADVICE_ID and a.CUST_ID = b.CUS_ID and b.CITY_ID = c.CITY_ID(+)) b,
	   (select TRANSP_ID, initcap(TRANSP_NAME) TRANSP_NAME from fb_transport) a,
       (select DTP_LOTNO,TMP_ID, DTPD_SRNOSTART, DTPD_SRNOEND, DTPD_INDWT, DTPD_CHESTWT,DTPD_ID
	        from fb_dailyteapacked a,fb_dtpdetails b where a.DTP_ID = b.DTP_ID)f,
	   (select initcap(UNIT_NAME) UNIT_NAME, UNIT_ADD from fb_gardenmaster where nvl(UNIT_ACTIVE_IND,'N') = 'Y') g
where c.SI_ID=d.SI_ID and d.DTPD_ID=f.DTPD_ID and f.TMP_ID=e.TMP_ID(+) and SI_TYPE = 'PRICONS' and
 	      c.TRANSP_ID = a.TRANSP_ID and c.ADVICE_ID = b.ADVICE_ID(+) and c.SI_ID= :ra_inv_no
group by c.ADVICE_ID,b.ADVICE_DATE,c.SI_ID,DTP_LOTNO,TMP_NAME ,c.SI_DATE,SI_CN,SI_CNDATE,TRANSP_NAME,CUS_NAME,UNIT_NAME,CITY_NAME,
