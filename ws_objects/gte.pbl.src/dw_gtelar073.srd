 retrieve="--spouse
select 'Spouse' Relation, initcap(FIELD_NAME) FIELD_NAME, nvl(RH_MANID, b.rh_id) line_name, house_id, EMP_RATIONCARDNO, EMP_ID, initcap(EMP_NAME) EMP_NAME , emp_sex, SPOUSE_ID dependent_id,  initcap(SPOUSE_NAME) dependent_name, SPOUSE_DOB, round(((to_date(:ra_dt,'dd/mm/yyyy') - SPOUSE_dob)/365), 1) dependent_age, null School_going, decode( nvl(spouse_active, '0'), '1', 'Yes', 'No') dependent_active_ind, decode(emp_sex, 'F', 'No', decode(nvl(spouse_active, '0'), '0', 'No', DECODE(NVL(RP_SPOUSE_IND, 'N'), 'N', 'No', decode(max(NVL(LRC_ADULTALLOW, 'NO')), 'NO', 'No', decode(nvl(spouse_id, 'X'), 'X', 'Yes', 'No'))))) Ration_entitled, LAST_RATION_GIVEN
from fb_labourspouse a, fb_employee b, fb_rationparam c, fb_labourrationchart d, fb_field e, fb_residentialhouse f
where a.LABOUR_ID = b.emp_id and RP_EMPTYPE = emp_type and b.field_id = e.field_id and b.rh_id = f.rh_id(+) and to_date(:ra_dt,'dd/mm/yyyy') between RP_PERIOD_FROM and nvl(RP_PERIOD_TO,trunc(sysdate))
AND to_date(:ra_dt,'dd/mm/yyyy')  between LRC_FRDT and nvl(LRC_TODT,trunc(sysdate)) AND LRC_LABOURTYPE =  DECODE(EMP_TYPE , 'LO', 'LT', EMP_TYPE) 
AND LRC_MAJORMINORFLAG = 'MAJOR' AND  EMP_TYPE IN ('LP', 'LT', 'LO')
AND NVL(EMP_ACTIVE, 0) = 1 AND EMP_INACTIVETYPE = 'Regular'
group by EMP_ID, EMP_NAME , SPOUSE_ID,  SPOUSE_NAME, RP_SPOUSE_IND, spouse_active, FIELD_NAME, nvl(RH_MANID, b.rh_id), house_id, EMP_RATIONCARDNO, LAST_RATION_GIVEN, emp_sex, SPOUSE_DOB
union all
--Male labour dependent
select initcap(CHILD_DEPENDENTTYPE) Relation, initcap(FIELD_NAME) FIELD_NAME, nvl(RH_MANID, a.rh_id) line_name, house_id, EMP_RATIONCARDNO, emp_id,  initcap(EMP_NAME) emp_name, emp_sex, null, initcap(CHILD_NAME) dependent_name, CHILD_DOB, round(((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365), 1) dependent_age, initcap(CHILD_SCHOOLFLAG) School_going, 'Yes' active_ind, 'Yes' ration_entitled, LAST_RATION_GIVEN
from fb_employee a, fb_labourdependent b, fb_rationparam c , fb_field d, fb_residentialhouse e
where a.emp_id  = b.LABOUR_ID  and RP_EMPTYPE = emp_type and a.field_id = d.field_id and a.rh_id = e.rh_id(+) and to_date(:ra_dt,'dd/mm/yyyy') between RP_PERIOD_FROM and nvl(RP_PERIOD_TO,trunc(sysdate)) and
EMP_TYPE IN ('LP', 'LT', 'LO') AND NVL(EMP_ACTIVE, 0) = 1 AND EMP_INACTIVETYPE = 'Regular' and RP_CH_IND = 'Y' and emp_sex = 'M' and emp_marital = 'M' and
nvl(child_active, 0) = 1 and CHILD_MARITAL = 'U' and 
((((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_WSCH_AGE) or
(((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_SCH_AGE and child_schoolflag='YES') or
(RP_UM_FCH='Y' and CHILD_DEPENDENTTYPE = 'CHILD' and CHILD_GENDER = 'F' ))
union all
-- female labour dependent
select initcap(CHILD_DEPENDENTTYPE) Relation, initcap(FIELD_NAME) FIELD_NAME, nvl(RH_MANID, a.rh_id) line_name, house_id, EMP_RATIONCARDNO, emp_id,  initcap(EMP_NAME) emp_name, emp_sex, null, initcap(CHILD_NAME) dependent_name, CHILD_DOB, round(((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365), 1) dependent_age, initcap(CHILD_SCHOOLFLAG) School_going, 'Yes' active_ind, 'Yes' ration_entitled, LAST_RATION_GIVEN
from fb_employee a, fb_labourdependent b, fb_rationparam c  , fb_field d, fb_residentialhouse e
where a.emp_id  = b.LABOUR_ID  and RP_EMPTYPE = emp_type and a.field_id = d.field_id and a.rh_id = e.rh_id(+) and to_date(:ra_dt,'dd/mm/yyyy') between RP_PERIOD_FROM and nvl(RP_PERIOD_TO,trunc(sysdate)) and
EMP_TYPE IN ('LP', 'LT', 'LO') AND NVL(EMP_ACTIVE, 0) = 1 AND EMP_INACTIVETYPE = 'Regular' and RP_CH_IND = 'Y' and emp_sex = 'F' and emp_marital = 'M' and
not exists (select lsp.labour_id labour_id  from fb_labourspouse lsp,fb_employee emp 
             where emp.emp_id=lsp.labour_id and emp.emp_type in ('LP','SS','ST') and spouse_id=a.emp_id  and spouse_active='1' and emp_active = '1') and
nvl(child_active, 0) = 1 and CHILD_MARITAL = 'U' and 
((((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_WSCH_AGE) or
(((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_SCH_AGE and child_schoolflag='YES') or
(RP_UM_FCH='Y' and CHILD_DEPENDENTTYPE = 'CHILD' and CHILD_GENDER = 'F' ))
union all
-- give female labour ration of spouse  
select initcap(CHILD_DEPENDENTTYPE)||' of Spouse' Relation, initcap(FIELD_NAME) FIELD_NAME, nvl(RH_MANID, a.rh_id) line_name, house_id, EMP_RATIONCARDNO, emp_id,  initcap(EMP_NAME) emp_name, emp_sex, null, initcap(CHILD_NAME) dependent_name, CHILD_DOB, round(((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365),1) dependent_age, initcap(CHILD_SCHOOLFLAG) School_going, 'Yes' active_ind, 'Yes' ration_entitled, b.LAST_RATION_GIVEN
from fb_employee a, fb_labourdependent b, fb_rationparam c, fb_labourspouse d , fb_field e, fb_residentialhouse f
where a.emp_id  = d.SPOUSE_ID and D.LABOUR_ID = b.LABOUR_ID  and RP_EMPTYPE = emp_type and a.field_id = e.field_id and a.rh_id = f.rh_id(+) and to_date(:ra_dt,'dd/mm/yyyy') between RP_PERIOD_FROM and nvl(RP_PERIOD_TO,trunc(sysdate)) and
EMP_TYPE IN ('LP', 'LT', 'LO') AND NVL(EMP_ACTIVE, 0) = 1 AND EMP_INACTIVETYPE = 'Regular' and RP_CH_IND = 'Y' and emp_sex = 'F' and emp_marital = 'M' and
not exists (select lsp.labour_id labour_id  from fb_labourspouse lsp,fb_employee emp 
             where emp.emp_id=lsp.labour_id and emp.emp_type in ('LP','SS','ST') and spouse_id=a.emp_id  and spouse_active='1' and emp_active = '1') and
not exists (select labour_id from fb_labourdependent where child_Active = 1 and LABOUR_ID = a.emp_id) and              
nvl(child_active, 0) = 1 and CHILD_MARITAL = 'U' and 
((((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_WSCH_AGE) or
(((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_SCH_AGE and child_schoolflag='YES') or
(RP_UM_FCH='Y' and CHILD_DEPENDENTTYPE = 'CHILD' and CHILD_GENDER = 'F' ))
union all
-- non eligible labour 
select initcap(CHILD_DEPENDENTTYPE) Relation, initcap(FIELD_NAME) FIELD_NAME, nvl(RH_MANID, a.rh_id) line_name, house_id, EMP_RATIONCARDNO, emp_id,  initcap(EMP_NAME)  emp_name, emp_sex, null, initcap(CHILD_NAME) dependent_name, CHILD_DOB, round(((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365),1) dependent_age, initcap(CHILD_SCHOOLFLAG) School_going, decode( nvl(child_active, '0'), '1', 'Yes', 'No') active_ind, 'No' ration_entitled, LAST_RATION_GIVEN
from fb_employee a, fb_labourdependent b, fb_field i, fb_residentialhouse j
where a.emp_id  = b.LABOUR_ID  and EMP_TYPE IN ('LP', 'LT', 'LO') AND NVL(EMP_ACTIVE, 0) = 1 AND EMP_INACTIVETYPE = 'Regular' and a.field_id = i.field_id and a.rh_id = j.rh_id(+) and
not exists (
    select 'CHILD' Relation, emp_id, emp_name, null, CHILD_NAME, 'Yes' active_ind, 'Yes' ration_entitled
    from fb_employee c, fb_labourdependent d, fb_rationparam e 
    where c.emp_id  = d.LABOUR_ID  and RP_EMPTYPE = emp_type and to_date(:ra_dt,'dd/mm/yyyy') between RP_PERIOD_FROM and nvl(RP_PERIOD_TO,trunc(sysdate)) and
    EMP_TYPE IN ('LP', 'LT', 'LO') AND NVL(EMP_ACTIVE, 0) = 1 AND EMP_INACTIVETYPE = 'Regular' and RP_CH_IND = 'Y' and emp_sex = 'M' and emp_marital = 'M' and
    nvl(child_active, 0) = 1 and CHILD_MARITAL = 'U' and 
    ((((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_WSCH_AGE) or
    (((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_SCH_AGE and child_schoolflag='YES') or
    (RP_UM_FCH='Y' and CHILD_DEPENDENTTYPE = 'CHILD' and CHILD_GENDER = 'F' ))
    and c.emp_id||d.child_name =  a.emp_id||b.child_name
    union all
    select 'CHILD' Relation, emp_id, emp_name, null, CHILD_NAME dependent_name, 'Yes' active_ind, 'Yes' ration_entitled
    from fb_employee f, fb_labourdependent g, fb_rationparam h 
    where f.emp_id  = g.LABOUR_ID  and RP_EMPTYPE = emp_type and to_date(:ra_dt,'dd/mm/yyyy') between RP_PERIOD_FROM and nvl(RP_PERIOD_TO,trunc(sysdate)) and
    EMP_TYPE IN ('LP', 'LT', 'LO') AND NVL(EMP_ACTIVE, 0) = 1 AND EMP_INACTIVETYPE = 'Regular' and RP_CH_IND = 'Y' and emp_sex = 'F' and emp_marital = 'M' and
    not exists (select lsp.labour_id labour_id  from fb_labourspouse lsp,fb_employee emp 
                 where emp.emp_id=lsp.labour_id and emp.emp_type in ('LP','SS','ST') and spouse_id=a.emp_id  and spouse_active='1' and emp_active = '1') and
    nvl(child_active, 0) = 1 and CHILD_MARITAL = 'U' and 
    ((((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_WSCH_AGE) or
    (((to_date(:ra_dt,'dd/mm/yyyy') - child_dob)/365) between RP_CH_STARTAGE and RP_CH_SCH_AGE and child_schoolflag='YES') or
    (RP_UM_FCH='Y' and CHILD_DEPENDENTTYPE = 'CHILD' and CHILD_GENDER = 'F' ))
    and f.emp_id||g.child_name =  a.emp_id||b.child_name    
)
order by field_name,  emp_name, relation desc, dependent_name
" arguments=(("ra_dt", string)) )
