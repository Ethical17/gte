 retrieve="SELECT initcap(FIELD_NAME) division,a.EMP_ID empid, initcap(a.EMP_NAME) employeename,
       a.EMP_DESIG empdesignation,
	   c.EP_BASICAMOUNT basicearn,
	   c.EP_DAAMOUNT empda,c.EP_VDAAMOUNT empvda,
   	   (nvl(c.EP_BASICAMOUNT,0) + nvl(c.EP_DAAMOUNT,0) + nvl(c.EP_VDAAMOUNT,0)) Totalamt,
       	to_char(to_date(:ra_month,'MM'),'Month') mon
  FROM FB_EMPLOYEE a, FB_NONEXEPERMAEMP b,	FB_EMPPAYMENT c, 
       FB_EMPBASICSTAGES d,FB_EMPBASICAPPLICATIONYEAR e, fb_field f
WHERE  a.EMP_ID = b.EMP_ID(+) AND a.EMP_ID = c.EMP_ID(+) AND 
       b.EBS_ID = d.EBS_ID AND a.FIELD_ID = f.FIELD_ID and 
	   a.FIELD_ID = decode(:ra_division,'ALL',a.FIELD_ID,:ra_division) and 
	   d.EBA_ID = e.EBA_ID AND b.EMP_GRADE = e.EBA_GRADE AND
	   a.EMP_TYPE = decode(:ra_emptype,'AE',a.EMP_TYPE,:ra_emptype) and 
	   c.EP_MONTH = :ra_month AND c.EP_YEAR = :ra_year AND 
	   e.EBA_VALIDITYFLAG = '1'" arguments=(("ra_division", string),("ra_year", number),("ra_month", number),("ra_emptype", string))  sort="division A empid A " )
