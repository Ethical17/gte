	round(decode(AP_NOOFSPLIT, 1, 0, 2,  nvl(EA_PF, 0)/2, 3, nvl(EA_PF, 0)/3),2) pf_second,
	round(decode(AP_NOOFSPLIT, 1, 0, 2, 0, 3, nvl(EA_AMOUNT, 0)/3),2) amt_third , 
	round(decode(AP_NOOFSPLIT, 1, 0, 2, 0, 3, nvl(EA_PF, 0)/3),2) pf_third,
	trunc(AP_STARTDATE) AP_STARTDATE, trunc(AP_ENDDATE) AP_ENDDATE, trunc(AP_INC_DATE) AP_INC_DATE, AP_NOOFSPLIT
from fb_emp_arrearperiod a, fb_emparrear b, fb_employee c
where  a.ap_id = b.ap_id and b.emp_id = c.emp_id and a.ap_id = :ra_ap_id
