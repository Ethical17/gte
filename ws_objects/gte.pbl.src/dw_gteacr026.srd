table(column=(type=char(25) updatewhereclause=yes key=yes name=emp_id dbname="fb_employee.emp_id" dbalias="a.emp_id" )
 column=(type=char(30) updatewhereclause=yes name=emp_name dbname="emp_name" )
 column=(type=char(10) updatewhereclause=yes name=emp_pfno dbname="fb_employee.emp_pfno" dbalias="a.emp_pfno" )
 column=(type=datetime updatewhereclause=yes name=lad_date dbname="fb_employee.lad_date" dbalias=".lad_date" )
 column=(type=number updatewhereclause=yes name=advbal dbname="advbal" )
 column=(type=number updatewhereclause=yes name=advissue dbname="advissue" )
 column=(type=number updatewhereclause=yes name=adv_ded dbname="adv_ded" )
 retrieve="SELECT a.EMP_ID, initcap(a.EMP_NAME) EMP_NAME, a.EMP_PFNO,
        LAD_DATE, sum(nvl(advbal,0)) advbal, sum(nvl(advissue,0)) advissue,sum(nvl(adv_ded,0)) adv_ded
 FROM   FB_EMPLOYEE a,
        (select null LAD_DATE,issue.labour_id,sum(nvl(issue.advissue,0)- (nvl(ded.advded,0) + nvl(ded.ADVANCEDEDCASH,0))) advbal, 0 advissue, 0 adv_ded
           from (select decode(lp12.labour_idphase1,NULL,labour_id,lp12.labour_idphase1) labour_id,sum(nvl(LA_PFADVANCE,0)) advissue
                    from fb_labouradvance la left outer join fb_labourphase1phase2 lp12 on (la.labour_id=lp12.labour_idphase1 or la.labour_id=lp12.labour_idphase2)
                 where LA_DATE < to_date(:ra_date,'dd/mm/yyyy') group by decode(lp12.labour_idphase1,NULL,labour_id,lp12.labour_idphase1) ) issue,
                  (select decode(lp12.labour_idphase1,NULL,labour_id,lp12.labour_idphase1) labour_id,sum(nvl(PFADVANCEDED,0)) advded, sum(nvl(PFADVDEDCASH,0)) ADVANCEDEDCASH
                    from fb_labadvancededuction lad left outer join fb_labourphase1phase2 lp12 on (lad.labour_id=lp12.labour_idphase1 or lad.labour_id=lp12.labour_idphase2) where LAD_DATE < to_date(:ra_date,'dd/mm/yyyy')
                    group by decode(lp12.labour_idphase1,NULL,labour_id,lp12.labour_idphase1) ) ded
                where issue.labour_id = ded.labour_id(+)
                group by issue.labour_id    
        union all
      select LAD_DATE, LABOUR_ID, 0 advbal, sum(decode(rec_ty,'iss',nvl(amt,0),0)) advissue, sum(decode(rec_ty,'ded',nvl(amt,0),0)) adv_ded
      from       
        (select 'ded' rec_ty,LAD_DATE,LABOUR_ID, (nvl(PFADVANCEDED,0) + nvl(PFADVDEDCASH,0)) amt  from fb_labadvancededuction where LAD_DATE >= to_date(:ra_date,'dd/mm/yyyy')
         union all
         select 'iss',la_date, decode(lp12.labour_idphase1,NULL,labour_id,lp12.labour_idphase1) labour_id,
                 sum(nvl(LA_PFADVANCE,0)) advissue
           from fb_labouradvance la left outer join fb_labourphase1phase2 lp12 on (la.labour_id=lp12.labour_idphase1 or la.labour_id=lp12.labour_idphase2)
         where la_date >= to_date(:ra_date,'dd/mm/yyyy')
         group by la_date,decode(lp12.labour_idphase1,NULL,labour_id,lp12.labour_idphase1) )
       group by LAD_DATE, LABOUR_ID) tran 
WHERE  a.emp_id = tran.labour_id(+) and 
       (nvl(advbal,0) <> 0 or nvl(advissue,0)  <> 0 or nvl(adv_ded,0)  <> 0) and a.EMP_ID = :ra_emp
group by a.EMP_ID, initcap(a.EMP_NAME), a.EMP_PFNO,LAD_DATE
order by  LAD_DATE" update="FB_EMPLOYEE" updatewhere=0 updatekeyinplace=no arguments=(("ra_date", string),("ra_emp", string)) )
