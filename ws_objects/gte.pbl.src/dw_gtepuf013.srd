       LPI_OTHERAMO, LPI_SALETAX, LPI_NETAMO, LPI_DISCOUNT, LPI_MANID, LPI_INSURANCE, LPI_CONFIRMFLAG, 
	   LPI_ENTRY_BY, LPI_ENTRY_DT,'N' del_flag,a.rowid , 'N' ind,LPI_VOU_NO,LPI_VOU_DT,lpi_billno, lpi_billdate,
         decode(LPI_VOU_NO,null,'N','Y') appr_flag,LPI_ISS_LOCN, LPI_REC_LOCN, LPI_rev_chrg, LPI_ISS_GSTNNO, LPI_REC_GSTNNO,lpi_rev_cat, a.lpo_id, b.lpo_date
from  fb_servicepurchaseinvoice a, fb_servicepurchaseorder b, fb_supplier d
where  a.SUP_ID=d.SUP_ID(+) and a.lpo_id = b.lpo_id 
