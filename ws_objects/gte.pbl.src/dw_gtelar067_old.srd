table(column=(type=char(25) updatewhereclause=yes name=emp_id dbname="emp_id" )
 column=(type=char(70) updatewhereclause=yes name=empname dbname="empname" )
 column=(type=number updatewhereclause=yes name=pfno dbname="pfno" )
 column=(type=number updatewhereclause=yes name=ctot_pf dbname="ctot_pf" )
 column=(type=number updatewhereclause=yes name=adv_made dbname="adv_made" )
 column=(type=number updatewhereclause=yes name=adv_ref dbname="adv_ref" )
 column=(type=number updatewhereclause=yes name=adv_int dbname="adv_int" )
 column=(type=datetime updatewhereclause=yes name=frdt dbname="frdt" )
 column=(type=datetime updatewhereclause=yes name=todt dbname="todt" )
 column=(type=number updatewhereclause=yes name=year_mon dbname="year_mon" )
 retrieve="select max(nvl(emp_id,'')) emp_id, max(empname) empname, pfno, round(sum(ctot_pf), 2) ctot_pf , round(sum(adv_made), 2) adv_made, round(sum(adv_ref), 2) adv_ref, round(sum(adv_int), 2) adv_int, frdt, todt, :ra_yrmn year_mon from
(
    SELECT  a.EMP_ID emp_id,initcap(a.EMP_NAME) empname,  to_number(a.EMP_PFNO) pfno ,       
             sum(nvl(b.LABOUR_PF,0) + nvl(b.LABOUR_FPF,0))*2 ctot_pf,
             nvl(adv_made,0) adv_made,
             sum(nvl(LABOUR_PFADVANCEDED,0)) adv_ref,
            nvl(adv_int,0) adv_int, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
    FROM FB_EMPLOYEE a, FB_LABOURWEEKLYWAGES b,FB_LABOURWAGESWEEK c,fb_field d, fb_labadvancededuction e,
        (select LABOUR_ID, sum(nvl(LA_PFADVANCE,0)) adv_made, sum(nvl(LA_PFINTEREST,0)) adv_int  from fb_labouradvance where LA_DATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and NVL(LA_DESC, 'X')<>'P.F. ADV. Acquisition Company' group by LABOUR_ID) f
    WHERE (a.EMP_ID=b.LABOUR_ID) AND (b.LWW_ID=c.LWW_ID) AND b.lww_id = e.lww_id(+) and b.LABOUR_ID = e.LABOUR_ID(+) and
          b.labour_id = f.labour_id(+) and 
            LWW_STARTDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and
            LWW_ENDDATE between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') and
           (nvl(b.LABOUR_PF,0) <> 0 OR nvl(b.LABOUR_FPF,0) <> 0 or nvl(adv_made,0) <> 0 or nvl(LABOUR_PFADVANCEDED,0) <> 0 or nvl(adv_int,0) <> 0) AND  LWW_PAIDFLAG = '1' and 
            nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type) and a.field_id = d.field_id and
           decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
    group by to_number(a.EMP_PFNO) ,a.EMP_ID, a.EMP_PFDEDCODE, initcap(a.EMP_NAME), a.EMP_INACTIVETYPE, nvl(adv_made,0),nvl(adv_int,0),nvl(adv_int,0) 
    union all
    SELECT  d.EMP_ID,initcap(d.EMP_NAME) empname,to_number(d.EMP_PFNO) pfno ,   
                sum(nvl(c.LWW_PF,0) + nvl(c.LWW_FPF,0))*2 ctot_pf,
                0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt        
        FROM   FB_LABLWWPERIOD a, FB_LABLWWSEASON b, FB_LABLWW c,FB_EMPLOYEE d , fb_field e
        WHERE  a.LLS_ID = b.LLS_ID AND a.LLP_ID = c.LLP_ID AND c.LABOUR_ID = d.EMP_ID AND d.field_id = e.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
              and LLS_PDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LWW_PF,0) + nvl(c.LWW_FPF,0)) <> 0
        group by d.EMP_ID,initcap(d.EMP_NAME) ,to_number(d.EMP_PFNO)  
    union all
    SELECT   a.EMP_ID,initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
           (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_1,0)/100)  ctot_pf , 
            0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
    FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
    WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
    and AP_PAYDT_1 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_1 is not null
    group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_1,0)
    union all
    SELECT   a.EMP_ID,initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
            (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_2,0)/100) ctot_pf , 
            0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
    FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
    WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
    and AP_PAYDT_2 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_2 is not null
    group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_2,0)
    union all
    SELECT   a.EMP_ID,initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
            (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_3,0)/100) ctot_pf , 
            0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
    FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
    WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
    and AP_PAYDT_3 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_3 is not null
    group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_3,0)
    union all
    select '',initcap(trim(emp_name)) emp_name, to_number(pf_no) pfno,
            sum(contribution), sum(advance_made), sum(advance_refund), sum(advance_interest), to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
        from fb_pfform1
        where frdt between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and
            todt between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy')
        group by initcap(trim(emp_name)) , to_number(pf_no) 
    union all
    SELECT   a.EMP_ID,initcap(a.EMP_NAME) empname, to_number(a.EMP_PFNO) pfno ,
             (nvl(EP_PENSIONFUNDDEDAMOUNT,0) + nvl(EP_PFCONTDEDAMOUNT,0))*2 epf_contribution_due,
             nvl(adv_made,0) adv_made,
             nvl(PFADVANCEDED,0) adv_ref,
             nvl(adv_int,0) adv_int, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
    FROM FB_EMPLOYEE a,fb_emppayment b, fb_field d, (select EMP_ID, sum(nvl(EWD_PFADVDED,0)) pfadvanceded  from FB_EMPWISEDEDUCTION where (nvl(EWD_YEAR,0) * 100 + nvl(EWD_MONTH,0))  = :ra_yrmn group by emp_id)  e,
             (select emp_ID, sum(nvl(emp_PFADVANCE,0)) adv_made, sum(nvl(emp_PFINTEREST,0)) adv_int  from fb_empadvance where to_char(emp_DATE,'YYYYMM') = :ra_yrmn and EMP_DESC<>'P.F. ADV. Acquisition Company'  group by emp_ID) f
    where a.EMP_ID = b.EMP_ID and a.emp_id = e.emp_id(+) and a.emp_id = f.emp_id(+) and ((nvl(EP_YEAR,0) * 100) + nvl(EP_MONTH,0)) = :ra_yrmn and 
            nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type) and
             a.field_id = d.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
    union all         
    SELECT  a.EMP_ID,initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno,sum(nvl(EA_PF,0) *2) ctot_pf,0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
    from FB_EMPLOYEE a, fb_emparrear b ,fb_emp_arrearperiod c
    where a.emp_id=b.EMP_ID and b.AP_ID=c.AP_ID and AP_PAYDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') 
    and nvl(EA_PF,0)<>0 and nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type)
    group by   a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO)
)
group by pfno, frdt, todt-- present in pf_form 1
union all
select distinct null, initcap(trim(nvl(b.EMP_NAME,a.emp_name))) emp_name, to_number(pf_no), 0, 0, 0, 0,  to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt, :ra_yrmn year_mon
from fb_pfform5 a, (select * from fb_employee where EMP_ACTIVE = '1') b where PF_NO = to_number(trim(emp_pfno(+))) and year in (:ra_pfyear-1, :ra_pfyear-2) and nvl(settlement_status,'N') = 'N' and upper(a.EMP_TYPE) not in ('SETTLEMENT', 'OUTGOING','UNCLAIMED')
and not exists (select emp_pfno from fb_pfsettled c where c.emp_pfno = to_number(pf_no) and PFS_SETTLEDT < to_date(:ra_frdt,'dd/mm/yyyy'))
and not exists (select emp_pfno from fb_pf_incomingoutgoing d where d.emp_pfno = to_number(pf_no) and  pfig_tag = 'OUTGOING' and pfig_date < to_date(:ra_frdt,'dd/mm/yyyy'))
and not exists (select emp_pfno from fb_pfunclaimed e where e.emp_pfno = to_number(pf_no) and PFU_DT < to_date(:ra_frdt,'dd/mm/yyyy'))
and not exists
(select pfno from
(SELECT  initcap(a.EMP_NAME) empname,  to_number(a.EMP_PFNO) pfno ,       
             sum(nvl(b.LABOUR_PF,0) + nvl(b.LABOUR_FPF,0))*2 ctot_pf,
             sum(nvl(adv_made,0)) adv_made,
             sum(nvl(LABOUR_PFADVANCEDED,0)) adv_ref,
             sum(nvl(adv_int,0)) adv_int, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
    FROM FB_EMPLOYEE a, FB_LABOURWEEKLYWAGES b,FB_LABOURWAGESWEEK c,fb_field d, fb_labadvancededuction e,
         (select LABOUR_ID, sum(nvl(LA_PFADVANCE,0)) adv_made, sum(nvl(LA_PFINTEREST,0)) adv_int  from fb_labouradvance where LA_DATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') group by LABOUR_ID) f
  WHERE (a.EMP_ID=b.LABOUR_ID) AND (b.LWW_ID=c.LWW_ID) AND b.lww_id = e.lww_id(+) and b.LABOUR_ID = e.LABOUR_ID(+) and
          b.labour_id = f.labour_id(+) AND 
            LWW_STARTDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and
            LWW_ENDDATE between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') and
           (nvl(b.LABOUR_PF,0) <> 0 OR nvl(b.LABOUR_FPF,0) <> 0 or nvl(adv_made,0) <> 0 or nvl(LABOUR_PFADVANCEDED,0) <> 0 or nvl(adv_int,0) <> 0) AND  LWW_PAIDFLAG = '1' and 
            nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type) and a.field_id = d.field_id and
           decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
group by to_number(a.EMP_PFNO) ,a.EMP_ID, a.EMP_PFDEDCODE, initcap(a.EMP_NAME), a.EMP_INACTIVETYPE
union all
SELECT  initcap(d.EMP_NAME) empname,to_number(d.EMP_PFNO) pfno ,   
            (nvl(c.LWW_PF,0) + nvl(c.LWW_FPF,0))*2 ctot_pf,
            0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt        
    FROM   FB_LABLWWPERIOD a, FB_LABLWWSEASON b, FB_LABLWW c,FB_EMPLOYEE d , fb_field e
    WHERE  a.LLS_ID = b.LLS_ID AND a.LLP_ID = c.LLP_ID AND c.LABOUR_ID = d.EMP_ID AND d.field_id = e.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
          and LLS_PDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LWW_PF,0) + nvl(c.LWW_FPF,0)) <> 0
union all
SELECT  initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
       (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_1,0)/100)  ctot_pf , 
        0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
and AP_PAYDT_1 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_1 is not null
group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_1,0)
union all
SELECT  initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
        (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_2,0)/100) ctot_pf , 
        0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
and AP_PAYDT_2 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_2 is not null
group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_2,0)
union all
SELECT  initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
        (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_3,0)/100) ctot_pf , 
        0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
and AP_PAYDT_3 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_3 is not null
group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_3,0)
union all
SELECT initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno,sum(nvl(EA_PF,0) *2) ctot_pf,0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
from FB_EMPLOYEE a, fb_emparrear b ,fb_emp_arrearperiod c
where a.emp_id=b.EMP_ID and b.AP_ID=c.AP_ID and AP_PAYDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') 
and nvl(EA_PF,0)<>0 and nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type)
group by  initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO)
union all
select initcap(trim(emp_name)) emp_name, to_number(pf_no) pfno,
        sum(contribution), sum(advance_made), sum(advance_refund), sum(advance_interest), to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
    from fb_pfform1
    where frdt between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and
        todt between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy')
    group by initcap(trim(emp_name)) , to_number(pf_no) 
union all
SELECT   initcap(a.EMP_NAME) empname, to_number(a.EMP_PFNO) pfno ,
         (nvl(EP_PENSIONFUNDDEDAMOUNT,0) + nvl(EP_PFCONTDEDAMOUNT,0))*2 epf_contribution_due,
         nvl(adv_made,0) adv_made,
         nvl(PFADVANCEDED,0) adv_ref,
         nvl(adv_int,0) adv_int, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
FROM FB_EMPLOYEE a,fb_emppayment b, fb_field d, (select EMP_ID, sum(nvl(EWD_PFADVDED,0)) pfadvanceded  from FB_EMPWISEDEDUCTION where (nvl(EWD_YEAR,0) * 100 + nvl(EWD_MONTH,0))  = :ra_yrmn group by emp_id)  e,
         (select emp_ID, sum(nvl(emp_PFADVANCE,0)) adv_made, sum(nvl(emp_PFINTEREST,0)) adv_int  from fb_empadvance where to_char(emp_DATE,'YYYYMM') = :ra_yrmn  group by emp_ID) f
where a.EMP_ID = b.EMP_ID and a.emp_id = e.emp_id(+) and a.emp_id = f.emp_id(+) and ((nvl(EP_YEAR,0) * 100) + nvl(EP_MONTH,0)) = :ra_yrmn and 
        nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type) and
         a.field_id = d.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf) x
where pfno = pf_no
group by initcap(trim(nvl(b.EMP_NAME,a.emp_name))) , pfno, frdt, todt)-- present in form 5 but not in form 1
union all
select emp_id,initcap(trim(emp_name)) empname, to_number(EMP_PFNO) pfno , 0, 0, 0, 0,  to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt, :ra_yrmn year_mon
from fb_employee a, 
(select LABOUR_ID,  min(lww_startdate) min_dt from fb_labourweeklywages wage, fb_labourwagesweek wageperiod where nvl(LABOUR_PF, 0) > 0 and wage.lww_id = wageperiod.lww_id group by labour_id
union all
select emp_id, to_Date('01'||to_char(min((ep_year*100)+ep_month)), 'ddyyyymm') from fb_emppayment where nvl(EP_PFCONTDEDAMOUNT, 0) > 0 group by emp_id) min_pfded_dt  
where a.emp_id = min_pfded_dt.labour_id(+) and nvl(min_dt, emp_jdate) > (select nvl(max(todt), '01-Jan-1980') from fb_pfform5)  and EMP_JDATE  <= to_date(:ra_todt,'dd/mm/yyyy')
and not exists (select distinct EMP_PFNO from fb_pfsettled b where b.emp_pfno = a.emp_pfno and PFS_SETTLEDT <= to_date(:ra_todt,'dd/mm/yyyy'))
and not exists (select distinct EMP_PFNO from fb_pf_incomingoutgoing c where c.emp_pfno = a.emp_pfno and PFIG_DATE <= to_date(:ra_todt,'dd/mm/yyyy') and PFIG_TAG = 'OUTGOING')
and not exists (select emp_pfno from fb_pfunclaimed e where e.emp_pfno = a.emp_pfno and PFU_DT < to_date(:ra_frdt,'dd/mm/yyyy'))
and emp_pfno is not null
and  not exists (select distinct pf_no from fb_pfform5 where pf_no = to_number(EMP_PFNO) and year in (:ra_pfyear-1, :ra_pfyear-2) )
and  not exists 
(select pfno from
(SELECT  initcap(a.EMP_NAME) empname,  to_number(a.EMP_PFNO) pfno ,       
             sum(nvl(b.LABOUR_PF,0) + nvl(b.LABOUR_FPF,0))*2 ctot_pf,
             sum(nvl(adv_made,0)) adv_made,
             sum(nvl(LABOUR_PFADVANCEDED,0)) adv_ref,
             sum(nvl(adv_int,0)) adv_int, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
    FROM FB_EMPLOYEE a, FB_LABOURWEEKLYWAGES b,FB_LABOURWAGESWEEK c,fb_field d, fb_labadvancededuction e,
         (select LABOUR_ID, sum(nvl(LA_PFADVANCE,0)) adv_made, sum(nvl(LA_PFINTEREST,0)) adv_int  from fb_labouradvance where LA_DATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') group by LABOUR_ID) f
  WHERE (a.EMP_ID=b.LABOUR_ID) AND (b.LWW_ID=c.LWW_ID) AND b.lww_id = e.lww_id(+) and b.LABOUR_ID = e.LABOUR_ID(+) and
          b.labour_id = f.labour_id(+)  AND 
            LWW_STARTDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and
            LWW_ENDDATE between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') and
           (nvl(b.LABOUR_PF,0) <> 0 OR nvl(b.LABOUR_FPF,0) <> 0 or nvl(adv_made,0) <> 0 or nvl(LABOUR_PFADVANCEDED,0) <> 0 or nvl(adv_int,0) <> 0) AND  LWW_PAIDFLAG = '1' and 
            nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type) and a.field_id = d.field_id and
           decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
group by to_number(a.EMP_PFNO) ,a.EMP_ID, a.EMP_PFDEDCODE, initcap(a.EMP_NAME), a.EMP_INACTIVETYPE
union all
SELECT  initcap(d.EMP_NAME) empname,to_number(d.EMP_PFNO) pfno ,   
            (nvl(c.LWW_PF,0) + nvl(c.LWW_FPF,0))*2 ctot_pf,
            0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt        
    FROM   FB_LABLWWPERIOD a, FB_LABLWWSEASON b, FB_LABLWW c,FB_EMPLOYEE d , fb_field e
    WHERE  a.LLS_ID = b.LLS_ID AND a.LLP_ID = c.LLP_ID AND c.LABOUR_ID = d.EMP_ID AND d.field_id = e.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
          and LLS_PDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LWW_PF,0) + nvl(c.LWW_FPF,0)) <> 0
union all
SELECT   initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
       (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_1,0)/100)  ctot_pf , 
        0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
and AP_PAYDT_1 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_1 is not null
group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_1,0)
union all
SELECT   initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
        (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_2,0)/100) ctot_pf , 
        0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
and AP_PAYDT_2 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_2 is not null
group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_2,0)
union all
SELECT   initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno, 
        (sum(nvl(c.LA_PF,0) + nvl(c.LA_FPF,0))*2)*(nvl(AP_PAYPER_3,0)/100) ctot_pf , 
        0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
FROM   FB_EMPLOYEE a, FB_LABOURARREAR c,FB_ARREARPERIOD d, fb_laboursheet e,  fb_field g
WHERE  a.EMP_ID = c.LABOUR_ID AND c.AP_ID = d.AP_ID  AND a.ls_id = e.ls_id and a.field_id = g.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf
and AP_PAYDT_3 between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and (nvl(c.LA_PF,0) + nvl(c.LA_FPF,0)) <> 0 and AP_PAYDT_3 is not null
group by a.EMP_ID,initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO) ,nvl(AP_PAYPER_3,0)
union all
SELECT initcap(a.EMP_NAME) emp_name,to_number(a.EMP_PFNO) pfno,sum(nvl(EA_PF,0) *2) ctot_pf,0,0,0, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt 
from FB_EMPLOYEE a, fb_emparrear b ,fb_emp_arrearperiod c
where a.emp_id=b.EMP_ID and b.AP_ID=c.AP_ID and AP_PAYDATE between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') 
and nvl(EA_PF,0)<>0 and nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type)
group by  initcap(a.EMP_NAME) ,to_number(a.EMP_PFNO)
union all
select initcap(trim(emp_name)) emp_name, to_number(pf_no) pfno,
        sum(contribution), sum(advance_made), sum(advance_refund), sum(advance_interest), to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
    from fb_pfform1
    where frdt between to_date(:ra_frdt,'dd/mm/yyyy') AND to_date(:ra_todt,'dd/mm/yyyy') and
        todt between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy')
    group by initcap(trim(emp_name)), to_number(pf_no)
union all
SELECT   initcap(a.EMP_NAME) empname, to_number(a.EMP_PFNO) pfno ,
         (nvl(EP_PENSIONFUNDDEDAMOUNT,0) + nvl(EP_PFCONTDEDAMOUNT,0))*2 epf_contribution_due,
         nvl(adv_made,0) adv_made,
         nvl(LABOUR_PFADVANCEDED,0) adv_ref,
         nvl(adv_int,0) adv_int, to_date(:ra_frdt,'dd/mm/yyyy') frdt, to_date(:ra_todt,'dd/mm/yyyy') todt
FROM FB_EMPLOYEE a,fb_emppayment b, fb_field d, (select EMP_ID, sum(nvl(EWD_PFADVDED,0)) LABOUR_PFADVANCEDED  from FB_EMPWISEDEDUCTION where (nvl(EWD_YEAR,0) * 100 + nvl(EWD_MONTH,0))  = :ra_yrmn group by emp_id)  e,
         (select emp_ID, sum(nvl(emp_PFADVANCE,0)) adv_made, sum(nvl(emp_PFINTEREST,0)) adv_int  from fb_empadvance where to_char(emp_DATE,'YYYYMM') = :ra_yrmn  group by emp_ID) f
where a.EMP_ID = b.EMP_ID and a.emp_id = e.emp_id(+) and a.emp_id = f.emp_id(+) and ((nvl(EP_YEAR,0) * 100) + nvl(EP_MONTH,0)) = :ra_yrmn and 
        nvl(a.EMP_INACTIVETYPE,'x') = decode(nvl(:ra_type,'ALL'),'ALL',nvl(a.EMP_INACTIVETYPE,'x'),:ra_type) and
         a.field_id = d.field_id and decode(nvl(FIELD_PFACCD,'x'),'x',:ra_gpf,FIELD_PFACCD) = :ra_pf)
where pfno = to_number(EMP_PFNO) 
group by emp_name, pfno, frdt, todt)" arguments=(("ra_frdt", string),("ra_todt", string),("ra_type", string),("ra_yrmn", number),("ra_gpf", string),("ra_pf", string),("ra_pfyear", number))  sort="pfno A " )
