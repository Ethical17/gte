 column=(type=char(25) updatewhereclause=yes name=acsubledgerid dbname="fb_acsubledger.acsubledgerid" )
 column=(type=char(78) updatewhereclause=yes name=eacsubledgername dbname="eacsubledgername" )
 column=(type=char(25) updatewhereclause=yes name=kamsub_id dbname="fb_expenseacsubhead.kamsub_id" )
