 retrieve="select acsub.acsubledger_name||' ('||acsub.acsubledger_id||')' acsubledgername,acsub.acsubledger_id acsubledgerid,
           EACSUBHEAD_NAME||' ('||KAMSUB_ID||')' eacsubledgername ,KAMSUB_ID,sum(nvl(dkw.wages,0)+nvl(dkw.elp,0)) grwages
   from (select lda.lww_id LWWID, lda.lda_date ATTENDATE, kam.kamsub_id KAMSUB_ID,
                sum(lda.lda_status) NODAYS,
		 		sum(decode(nvl(LDA_ENTRY_BY,'ADMIN'),'ADMIN',nvl(lda_wages,0),(nvl(lda_wages,0)-nvl(lda_elp,0)))) WAGES,
   		 		sum(lda.lda_elp) ELP 
		 from fb_labourdailyattendance lda,fb_kamsubhead kam
		 where lda.kamsub_id = kam.kamsub_id and lda.lww_id =:ra_lww_id and 	nvl(KAMSUB_ACTIVE_IND,'N') = 'Y' and  
		 ((to_char(to_date(:ra_fr_dt,'dd/mm/yyyy'),'mm') != to_char(to_date(:ra_to_dt,'dd/mm/yyyy'),'mm') and 
		    lda.lda_date >= LAST_DAY(to_date(:ra_fr_dt,'DD/MM/YYYY'))+1) or
	   	   (to_char(to_date(:ra_fr_dt,'dd/mm/yyyy'),'mm')=to_char(to_date(:ra_to_dt,'dd/mm/yyyy'),'mm') and 
		    lda.lda_date >= to_date(:ra_fr_dt,'DD/MM/YYYY')))
	    	  and lda.lda_date <= to_date(:ra_to_dt,'DD/MM/YYYY')
		 group by lda.lww_id,lda.lda_date,kam.kamsub_id 
		 having sum(decode(nvl(LDA_ENTRY_BY,'ADMIN'),'ADMIN',nvl(lda_wages,0),(nvl(lda_wages,0)-nvl(lda_elp,0)))) != 0) dkw,
   		 fb_expenseacsubhead eacsub, fb_acsubledger acsub 
 where dkw.kamsub_id = eacsub.eacsubhead_id and eacsub.eachead_id = acsub.acsubledger_id and 
	   nvl(EXP_HEAD_IND,'N') = 'Y' 
 group by acsub.acsubledger_name||' ('||acsub.acsubledger_id||')' ,acsub.acsubledger_id ,EACSUBHEAD_NAME||' ('||KAMSUB_ID||')',KAMSUB_ID" arguments=(("ra_lww_id", string),("ra_fr_dt", string),("ra_to_dt", string))  sort="acsubledgername A eacsubledgername A " )
