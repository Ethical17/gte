table(column=(type=char(25) updatewhereclause=yes name=tmp_name dbname="tmp_name" )
 column=(type=char(25) updatewhereclause=yes name=tmp_id dbname="tmp_id" )
 column=(type=char(25) updatewhereclause=yes name=tpc_id dbname="tpc_id" )
 column=(type=number updatewhereclause=yes name=sortstocktodate dbname="sortstocktodate" )
 retrieve="select TMP_NAME,a.TMP_ID, a.TPC_ID, ( nvl(dstp.sorttodate,0) + nvl(trans_qnty,0) - nvl(dtp1.packtodate,0) - nvl(sds_qnty,0) + nvl(tt_quantity,0)) sortstocktodate 
from 
    (select tmp_id, sum(nvl(dtmp_sortquantodayty,0)) sorttodate from fb_dailysortedteamadeproduct where dtmp_sortdate<= trunc(sysdate)
      group by tmp_id) dstp,
    (select dtpd.tmp_id, sum(nvl((dtpd.dtpd_srnoend-dtpd.dtpd_srnostart+1)*dtpd.dtpd_indwt,0)) -sum(nvl(changepacktodate,0)) packtodate from fb_dailyteapacked dtp,fb_dtpdetails dtpd ,(select a.dtp_id,sum(nvl((b.dtpd_srnoend-b.dtpd_srnostart+1)*b.dtpd_indwt,0)) changepacktodate from fb_dailyteapacked_new a,fb_dtpdetails_new  b where  a.dtp_id=b.dtp_id and    dtp_date<= trunc(sysdate) group by a.dtp_id) packchange
      where dtp.dtp_id=dtpd.dtp_id and nvl(dtp_location,'RE') = 'RE' and  dtp_date<= trunc(sysdate) and dtp.dtp_id=packchange.dtp_id(+)
      group by dtpd.tmp_id) dtp1,
     (select TPC_ID,TMP_ID, sum(nvl(SD_QNTY,0)) sds_qnty from fb_sample_dispatch 
            where SD_TYPE = 'S' group by TPC_ID,TMP_ID) samplediss,
     (select  TPC_ID,TMP_ID, sum(nvl(PT_QUANTITY,0)) trans_qnty  from fb_packteatransfer 
              where pt_trantype = 'R' and pt_status = 'S' group by TPC_ID,TMP_ID) madeteareceive, 
     ( select tmp_id, sum(decode(TT_LOCATION, 'TT' ,(-1) * nvl(TT_QUANTITY,0), 'RE', nvl(TT_QUANTITY,0) )) tt_quantity from 
     fb_teatransfer where tt_date<= trunc(sysdate) group by tmp_id) teatransfer,         
    (select distinct TMP_ID, TMP_NAME, TPC_ID from fb_teamadeproduct where nvl(TMP_ACTIVE_IND,'Y') = 'Y') a
where dstp.tmp_id = dtp1.tmp_id(+) and dstp.tmp_id(+) = a.tmp_id and a.tmp_id = samplediss.tmp_id(+) and dstp.tmp_id = madeteareceive.tmp_id(+) and a.tmp_id = teatransfer.tmp_id(+) and 
      ( nvl(dstp.sorttodate,0) + nvl(trans_qnty,0) - nvl(dtp1.packtodate,0) - nvl(sds_qnty,0) + nvl(tt_quantity,0) )  > 0"  sort="tmp_id A sortstocktodate D " )
