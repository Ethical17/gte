 column=(type=number updatewhereclause=yes name=unit dbname="fb_laboursheet.unit" dbalias=".unit" )
 retrieve="SELECT e.FIELD_NAME divisionname,emp_pfno,c.LS_ID paybookno, LS_LABOURTYPE,c.LS_DESC,EMP_TYPE, c.LS_manid, labourid, labourname, to_date(:ra_yrmn,'YYYYMM') yrmn,labour_koil, unit
FROM    FB_LABOURSHEET c, FB_FIELD e,
		  (select emp_pfno,FIELD_ID,a.LS_ID,LABOUR_ID labourid,initcap(EMP_NAME) labourname,EMP_TYPE,labour_koil from fb_labourweeklywages a, fb_labourwagesweek b, FB_EMPLOYEE 
		    where EMP_ID = LABOUR_ID and a.LWW_ID = b.LWW_ID and to_char(LWW_STARTDATE,'yyyymm') = :ra_yrmn and  labour_koil > 0 and
					EMP_TYPE in ('LP') and EMP_ACTIVE = '1') a,
		  (select nvl(KI_QUANTITY,0) unit from fb_kerosinissue where KI_YEARMONTH = :ra_yrmn)
WHERE  a.LS_ID = c.LS_ID(+) AND a.FIELD_ID = e.FIELD_ID " arguments=(("ra_yrmn", string))  sort="divisionname A paybookno A labourid A " )
