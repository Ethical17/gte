 GROUP BY C.ACSUBLEDGER_NAME, A.EACSUBHEAD_MANID,decode(nvl(e.SSP_ID,'x'),'x',SP_NAME,h.SSP_NAME),D.PRIS_CONFIRMFLAG, D.PRIS_ACTIVE, E.SP_UNIT,
