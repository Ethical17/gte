 retrieve=" SELECT C.ACSUBLEDGER_NAME, A.EACSUBHEAD_MANID,decode(nvl(e.SSP_ID,'x'),'x',SP_NAME,h.SSP_NAME)SP_NAME, SUM(B.PRIS_ISSUEQUANTITY)ISSUEQUANTITY, 
		     SUM(B.PRIS_VALUE) VALUE, SUM(B.PRIS_VALUE)/SUM(B.PRIS_ISSUEQUANTITY) RATE, 
		     D.PRIS_CONFIRMFLAG, D.PRIS_ACTIVE, E.SP_UNIT,ACLEDGER_LEDGERTYPE,ACSUBLEDGER_ID,
			 (G.budtoday) budtoday, (budqntytoday) budqntytoday,(G.fulbudget) fulbudget,(fulbudgetqnty) fulbudgetqnty, initcap(FIELD_NAME) division
   FROM FB_EXPENSEACSUBHEAD A, FB_PRODUCTISSUEDETAILS B, fb_acsubledger C,  fb_acledger f,FB_PRODUCTISSUE D, FB_STOREPRODUCT E,fb_storeproduct_std h,
		(select SP_ID, EACSUBHEAD_ID,
			    sum(decode(sign(((MSB_YEAR * 100) + msb_month) - to_number(TO_CHAR(TO_DATE(:ra_FR_dt,'DD/MM/YYYY'),'YYYYMM'))),-1,0,decode(sign(((MSB_YEAR * 100) + msb_month) - to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'YYYYMM'))),1,0,nvl(price,0)))) budtoday,
				sum(decode(sign(((MSB_YEAR * 100) + msb_month) - to_number(TO_CHAR(TO_DATE(:ra_FR_dt,'DD/MM/YYYY'),'YYYYMM'))),-1,0,decode(sign(((MSB_YEAR * 100) + msb_month) - to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'YYYYMM'))),1,0,nvl(QUANTITY,0)))) budqntytoday, 
			    sum(nvl(price,0)) fulbudget,	
				sum(nvl(QUANTITY,0))fulbudgetqnty
			from (select (MSB_YEAR + 1) MSB_YEAR, 1 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_JANPRICE,0) price,NVL(MSB_JANQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
					 select (MSB_YEAR + 1) MSB_YEAR, 2 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_FEBPRICE,0) price,NVL(MSB_FEBQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
					 select (MSB_YEAR + 1) MSB_YEAR, 3 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_MARPRICE,0) price,NVL(MSB_MARQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
					 select MSB_YEAR, 4 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_APRPRICE,0) price,NVL(MSB_APRQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
					 select MSB_YEAR, 5 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_MAYPRICE,0) price,NVL(MSB_MAYQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
					 select MSB_YEAR, 6 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_JUNPRICE,0) price,NVL(MSB_JUNQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
					 select MSB_YEAR, 7 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_JULPRICE,0) price,NVL(MSB_JULQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
