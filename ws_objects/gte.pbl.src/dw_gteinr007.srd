					 select MSB_YEAR, 9 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_SEPPRICE,0) price,NVL(MSB_SEPQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
