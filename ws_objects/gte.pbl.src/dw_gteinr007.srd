					 select MSB_YEAR, 11 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_NOVPRICE,0) price,NVL(MSB_NOVQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) union all
					 select MSB_YEAR, 12 msb_month,SP_ID, EACSUBHEAD_ID,nvl(MSB_DECPRICE,0) price,NVL(MSB_DECQUANTITY,0) QUANTITY from FB_MONTHLYSTOREBUDGET where MSB_YEAR = decode(sign(to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'mm')) - 4),-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))-1,to_number(TO_CHAR(TO_DATE(:ra_to_dt,'DD/MM/YYYY'),'yyyy'))) )	
 		  group by SP_ID, EACSUBHEAD_ID) G, fb_field h 	  
 WHERE  (A.EACSUBHEAD_ID=B.EACSUBHEAD_ID) AND (A.EACHEAD_ID=C.ACSUBLEDGER_ID(+)) AND (B.PRIS_ID=D.PRIS_ID) AND
			(B.SP_ID=E.SP_ID) AND  e.SSP_ID=h.ssp_id(+) AND D.PRIS_CONFIRMFLAG='1' AND D.PRIS_ACTIVE='1' AND 
