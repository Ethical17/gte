table(column=(type=char(25) updatewhereclause=yes name=lpo_id dbname="lpo_id" )
 column=(type=datetime updatewhereclause=yes name=lpo_date dbname="lpo_date" )
 retrieve="select distinct x.LPO_ID LPO_ID,x.LPO_DATE LPO_DATE
 from  (select b.sm_id,LPO_DATE, sum(nvl(LPO_QUANTITY,0))po_qnty,a.LPO_ID LPO_ID
    		   from fb_servicepurchaseorder a,fb_spodetails b
   		where a.LPO_ID=b.LPO_ID 
   		group by b.sm_id,a.LPO_ID,LPO_DATE)x,
 		(select d.sm_id, null LPO_DATE ,sum(nvl(LPI_QUANTITY,0))invoice_qnty,c.LPO_ID LPO_ID
    		   from fb_servicepurchaseinvoice c,fb_spidetails d
   		where c.LPI_ID=d.LPI_ID and LPO_ID IS NOT NULL 
   		group by d.sm_id,c.LPO_ID)y
where  x.LPO_ID=y.LPO_ID(+) and x.sm_id = y.sm_id(+) and nvl(y.invoice_qnty,0) < nvl(x.po_qnty,0) 
"  sort="lpo_date D lpo_id A " )
