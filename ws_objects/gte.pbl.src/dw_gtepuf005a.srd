 retrieve="select  b.rowid,b.HOPI_ID,d.SPC_ID, b.SP_ID SP_ID, HOPI_QUANTITY, HOPI_UNITPRICE, HOPI_DISCOUNT, 
           HOPI_EFFECTIVEUNITPRICE,b.HOPI_SALETAX,'N' del_flag,
          decode(nvl(b.HOPI_CESS_RATE,0),0,(hopi_effectiveunitprice* HOPI_QUANTITY),(nvl(hopi_effectiveunitprice,0) *  nvl(HOPI_QUANTITY,0))+ (nvl(HOPI_CESS_RATE,0) * nvl(HOPI_QUANTITY,0))+((hopi_effectiveunitprice* HOPI_QUANTITY)*(b.HOPI_SALETAX/100))) Amount,
          HOPI_QUANTITY as old_qnty,HOPI_VOU_DT,HOPI_CGST_PER, HOPI_CGST_AMT, HOPI_SGST_PER, HOPI_SGST_AMT, HOPI_IGST_PER, HOPI_IGST_AMT,HOPI_HSN_NO, HOPI_CESS_RATE, HOPI_CESS_AMOUNT
from fb_hopurchaseinvoice a,fb_hopidetails b,fb_storeproduct d
where a.HOPI_ID=b.HOPI_ID and b.HOPI_ID = :ra_ref_no and b.SP_ID=d.SP_ID(+)" update="fb_hopidetails" updatewhere=0 updatekeyinplace=yes arguments=(("ra_ref_no", string))  sort="rowid A " )
