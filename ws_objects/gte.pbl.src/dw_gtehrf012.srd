table(column=(type=char(25) updatewhereclause=yes name=empid dbname="empid" )
 column=(type=char(50) updatewhereclause=yes name=emp_name dbname="emp_name" )
 column=(type=number updatewhereclause=yes name=totearn dbname="totearn" )
 column=(type=number updatewhereclause=yes name=pensionded dbname="pensionded" )
 column=(type=number updatewhereclause=yes name=pfcontded dbname="pfcontded" )
 column=(type=number updatewhereclause=yes name=advbal dbname="advbal" )
 column=(type=number updatewhereclause=yes name=pfadvbal dbname="pfadvbal" )
 column=(type=number updatewhereclause=yes name=pfintbal dbname="pfintbal" )
 column=(type=number updatewhereclause=yes name=elecadvbal dbname="elecadvbal" )
 column=(type=number updatewhereclause=yes name=medadvbal dbname="medadvbal" )
 column=(type=number updatewhereclause=yes name=festadvbal dbname="festadvbal" )
 column=(type=number updatewhereclause=yes name=pfint_ded dbname="pfint_ded" )
 column=(type=number updatewhereclause=yes name=adv_ded dbname="adv_ded" initial="0" validation="double(gettext()) >=0 and double(gettext())<=  advbal and double(gettext()) <= totearn" validationmsg="'Value Should Not Be > Advance Balance & Net Pay, Please Check !!!'" )
 column=(type=number updatewhereclause=yes name=pfadv_ded dbname="pfadv_ded" initial="0" validation="double(gettext()) >=0 and double(gettext())<= pfadvbal and double(gettext()) <= totearn" validationmsg="'Value Should Not Be > PF Advance Balance & Net Pay, Please Check !!!'" )
 column=(type=number updatewhereclause=yes name=ewd_electricded dbname="ewd_electricded" )
 column=(type=number updatewhereclause=yes name=ewd_medadvded dbname="ewd_medadvded" )
 column=(type=number updatewhereclause=yes name=ewd_festadvded dbname="ewd_festadvded" )
 column=(type=number update=yes updatewhereclause=yes name=ewd_lipded dbname="ewd_lipded" initial="0" validation="double(gettext()) >=0 and double(gettext()) <= netpay and natpayafterded >= 0" validationmsg="'Value Should Not Be >  Net Pay, Please Check !!!'" )
 column=(type=char(1) updatewhereclause=yes name=exempt_ind dbname="exempt_ind" values="	Y/	N" )
 column=(type=char(6) updatewhereclause=yes name=ls_ym dbname="ls_ym" )
 retrieve="select lda.emp_id empid,emp_name,totearn,pensionded,pfcontded,advbal,pfadvbal,pfintbal, elecadvbal, medadvbal, festadvbal,nvl(EWD_pfinterestded,0)pfint_ded,
        nvl(advded,0) adv_ded, nvl(ewd_pfadvded,0) pfadv_ded,  nvl(ewd_electricded,0) ewd_electricded,  nvl(ewd_medadvded,0) ewd_medadvded,  nvl(ewd_festadvded,0) ewd_festadvded,
        nvl(EWD_LIPDED,nvl(EMP_LIP,0)) EWD_LIPDED,'Y' exempt_ind,to_char(to_date(:ra_ym,'yyyymm'),'yyyymm')  ls_ym
 from (select emp.emp_id,emp_name,EMP_LIP,
       (emp.ebs_basicamount +
       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_da,0) / 100) +
       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_vda,0) / 100) +
       decode(:ra_gsnm,'FB',0,(nvl(emp.ebs_basicamount,0) * nvl(ead.ead_hr,0) / 100)) + nvl(emp.emp_adhoc,0)) totearn,
       decode(emp.emp_pfdedcode,'2',((emp.ebs_basicamount +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_da,0) / 100) +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_vda,0) / 100) +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_hr,0) / 100) + nvl(ead.ead_cvrforpf,0)) * ead.ead_pensionfundded / 100),
                                    ((emp.ebs_basicamount +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_da,0) / 100) +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_vda,0) / 100) +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_hr,0) / 100) + nvl(emp.emp_adhoc,0) + nvl(ead.ead_cvrforpf,0)) * (nvl(ead.ead_pfcontded,0) + nvl(ead.ead_pensionfundded,0)) / 100))  pensionded,
       decode(emp.emp_pfdedcode,'2',(((emp.ebs_basicamount +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_da,0) / 100) +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_vda,0) / 100) +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_hr,0) / 100) + nvl(ead.ead_cvrforpf,0)) * (nvl(ead.ead_pfcontded,0) + nvl(ead.ead_pensionfundded,0)) / 100) -
                                     ((emp.ebs_basicamount +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_da,0) / 100) +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_vda,0) / 100) +
                                       (nvl(emp.ebs_basicamount,0) * nvl(ead.ead_hr,0) / 100) + nvl(ead.ead_cvrforpf,0)) * ead.ead_pensionfundded / 100)),
                                    0)  pfcontded
  from fb_employee emp,
           (select * from fb_empallowancededuction where (EAD_GRADE,(nvl(EAD_YEAR,0) * 100 + nvl(EAD_MONTH,0))) in
                  (select EAD_GRADE,max(nvl(EAD_YEAR,0) * 100 + nvl(EAD_MONTH,0)) from fb_empallowancededuction group by EAD_GRADE)) ead
  where emp.emp_grade=ead.ead_grade and ead.ead_validityflag='1' and 
              emp.emp_active='1' and     emp.emp_type in('EXE','SS','ST','AT')) lda,
        (select EMP_ID,(nvl(EWD_ADVANCEDED,0)) advded, (nvl(ewd_pfadvded,0)) ewd_pfadvded,  (nvl(EWD_LIPDED,0)) EWD_LIPDED,  (nvl(EWD_ELECTRICDED,0)) EWD_ELECTRICDED ,  (nvl(EWD_MEDADVDED,0)) EWD_MEDADVDED ,  (nvl(EWD_FESTADVDED,0)) EWD_FESTADVDED,(nvl(EWD_pfinterestded,0)) EWD_pfinterestded  
                    from fb_empwisededuction  where ((ewd_year * 100) + ewd_month) = :ra_ym and EWD_VOU_SAL = 'S') dedn,
(select issue.emp_id,(nvl(issue.advissue,0)-nvl(ded.advded,0))  advbal, (nvl(issue.pfadvissue,0)-nvl(ded.ewd_pfadvded,0)) pfadvbal, (nvl(issue.elecadvissue,0)-nvl(ded.ewd_electricded,0)) elecadvbal, (nvl(issue.pfintissue,0) - nvl(ded.pfintded,0)) pfintbal,
     (nvl(issue.medadvissue,0)-nvl(ded.ewd_medadvded,0)) medadvbal, (nvl(issue.festadvissue,0)-nvl(ded.ewd_festadvded,0)) festadvbal 
         from (select emp_id,sum(nvl(EMP_ADVANCEAMOUNT,0)) advissue, sum(nvl(EMP_PFADVANCE,0)) pfadvissue, sum(nvl(EMP_ELECADVANCE,0)) elecadvissue, sum(nvl(EMP_MEDADVANCE,0)) medadvissue, sum(nvl(EMP_FESTADVANCE,0)) festadvissue,sum(nvl(EMP_PFINTEREST,0)) pfintissue
                    from fb_empadvance 
                    group by emp_id) issue,
                  (select EMP_ID,sum(nvl(EWD_ADVANCEDED,0)) advded, sum(nvl(ewd_pfadvded,0)) ewd_pfadvded, sum(nvl(EWD_LIPDED,0)) EWD_LIPDED,  sum(nvl(EWD_ELECTRICDED,0)) EWD_ELECTRICDED , sum(nvl(EWD_MEDADVDED,0)) EWD_MEDADVDED , sum(nvl(EWD_FESTADVDED,0)) EWD_FESTADVDED, 
                    sum(nvl(EWD_pfinterestded,0)) pfintded 
                    from fb_empwisededuction  where  ((((ewd_year * 100) + ewd_month) < :ra_ym and EWD_VOU_SAL = 'S') or (((ewd_year * 100) + ewd_month) <= :ra_ym and EWD_VOU_SAL = 'V'))
                    group by emp_id) ded
        where issue.emp_id = ded.emp_id(+)) adv
where lda.emp_id = dedn.emp_id(+)  and lda.emp_id = adv.emp_id(+)  " arguments=(("ra_ym", string),("ra_gsnm", string))  sort="emp_name A empid A " )
