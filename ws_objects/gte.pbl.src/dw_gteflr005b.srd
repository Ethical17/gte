		 FROM (select SPR_DATE,(sum(nvl(SPR_GL,0) * nvl(SPR_PLKRATE,0)) / nvl(KAMSUB_AFRATE,0)) cash_md
					 from FB_SECTIONPLUCKINGROUND,(select KAMSUB_AFRATE from fb_kamsubhead where  KAMSUB_ID = 'ESUB0163' and KAMSUB_ID = 'ESUB0163' and nvl(KAMSUB_ACTIVE_IND,'N') = 'Y' and KAMSUB_TYPE='PLUCK'  and to_date(:ra_frdt,'dd/mm/yyyy') between KAMSUB_FRDT and nvl(KAMSUB_TODT,trunc(sysdate)))
