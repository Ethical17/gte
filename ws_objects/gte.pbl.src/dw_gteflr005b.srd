				 sum(decode(sign(TASK_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(TASK_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),1,0,(nvl(TASK_OMATODAYTY,0) + nvl(TASK_OFATODAYTY,0) +((nvl(TASK_OMADTODAYTY,0) + nvl(TASK_OFADTODAYTY,0) + nvl(TASK_OMCTODAYTY,0) + nvl(TASK_OFCTODAYTY,0)) * 0.5) )))) ltoday, 
