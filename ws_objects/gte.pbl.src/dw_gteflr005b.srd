					 TASK_DATE between to_date(:ra_ly_ysdt,'dd/mm/yyyy') and to_date(:ra_ly_frdt,'dd/mm/yyyy') )
		group by 30,'Green Leaf (Outsider)'
		union all
	   	SELECT 35 so, 'Green Leaf (Cash)' recdesc,
			 	 sum(decode(sign(SPR_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),-1,0,decode(sign(SPR_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),1,0,nvl(SPR_GL,0) ))) ctoday, 
		   		 sum(decode(sign(SPR_DATE - to_date(:ra_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(SPR_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),1,0,nvl(SPR_GL,0) )))  ctodate,
				 sum(decode(sign(SPR_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(SPR_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),1,0,nvl(SPR_GL,0) ))) ltoday, 
		   		 sum(decode(sign(SPR_DATE - to_date(:ra_ly_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(SPR_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),1,0,nvl(SPR_GL,0) ))) ltodate
		 FROM FB_SECTIONPLUCKINGROUND 
		 WHERE SPR_PLUCCKTYPE='C' and SECTION_ID in ( select SECTION_ID from fb_section where field_id = decode(:ra_field,'ALL',field_id,:ra_field)) and
		 			(SPR_DATE between to_date(:ra_ysdt,'dd/mm/yyyy') and to_date(:ra_frdt,'dd/mm/yyyy') or 
					 SPR_DATE between to_date(:ra_ly_ysdt,'dd/mm/yyyy') and to_date(:ra_ly_frdt,'dd/mm/yyyy') )
		group by 35,'Green Leaf (Cash)' 
		union all									
	   	SELECT   40 so, 'Mandays (Permanent)', 
			 	 sum(decode(sign(TASK_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),-1,0,decode(sign(TASK_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),1,0,(nvl(TASK_PMATODAYTY,0) + nvl(TASK_PFATODAYTY,0) + ((nvl(TASK_PMADTODAYTY,0) + nvl(TASK_PFADTODAYTY,0) + nvl(TASK_PMCTODAYTY,0) + nvl(TASK_PFCTODAYTY,0)) * 0.5) )))) ctoday, 
		   		 sum(decode(sign(TASK_DATE - to_date(:ra_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(TASK_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),1,0,(nvl(TASK_PMATODAYTY,0) + nvl(TASK_PFATODAYTY,0) +((nvl(TASK_PMADTODAYTY,0) + nvl(TASK_PFADTODAYTY,0) + nvl(TASK_PMCTODAYTY,0) + nvl(TASK_PFCTODAYTY,0)) * 0.5) )))) ctodate,
				 sum(decode(sign(TASK_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(TASK_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),1,0,(nvl(TASK_PMATODAYTY,0) + nvl(TASK_PFATODAYTY,0) + ((nvl(TASK_PMADTODAYTY,0) + nvl(TASK_PFADTODAYTY,0) + nvl(TASK_PMCTODAYTY,0) + nvl(TASK_PFCTODAYTY,0)) * 0.5) )))) ltoday, 
		   		 sum(decode(sign(TASK_DATE - to_date(:ra_ly_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(TASK_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),1,0,(nvl(TASK_PMATODAYTY,0) + nvl(TASK_PFATODAYTY,0) + ((nvl(TASK_PMADTODAYTY,0) + nvl(TASK_PFADTODAYTY,0) + nvl(TASK_PMCTODAYTY,0) + nvl(TASK_PFCTODAYTY,0)) * 0.5) )))) ltodate
		 FROM FB_TASKACTIVEDAILY b
		 WHERE  SECTION_ID in ( select SECTION_ID from fb_section where field_id = decode(:ra_field,'ALL',field_id,:ra_field)) and
                TASK_ID in (select kamsub_id from FB_KAMSUBHEAD where nvl(KAMSUB_ACTIVE_IND,'N') = 'Y' and KAMSUB_TYPE = 'PLUCK') and 
					(TASK_DATE between to_date(:ra_ysdt,'dd/mm/yyyy') and to_date(:ra_frdt,'dd/mm/yyyy') or 
					 TASK_DATE between to_date(:ra_ly_ysdt,'dd/mm/yyyy') and to_date(:ra_ly_frdt,'dd/mm/yyyy') )
		group by 40,'Mandays (Permanent)' 
		union all						
	   	SELECT   50 so, 'Mandays (Temporary)', 
			 	 sum(decode(sign(TASK_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),-1,0,decode(sign(TASK_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),1,0,(nvl(TASK_TMATODAYTY,0) + nvl(TASK_TFATODAYTY,0) + ((nvl(TASK_TMADTODAYTY,0) + nvl(TASK_TFADTODAYTY,0) + nvl(TASK_TMCTODAYTY,0) + nvl(TASK_TFCTODAYTY,0)) *0.5) )))) ctoday, 
		   		 sum(decode(sign(TASK_DATE - to_date(:ra_ysdt,'dd/mm/yyyy')),-1,0,decode(sign(TASK_DATE - to_date(:ra_frdt,'dd/mm/yyyy')),1,0,(nvl(TASK_TMATODAYTY,0) + nvl(TASK_TFATODAYTY,0) + ((nvl(TASK_TMADTODAYTY,0) + nvl(TASK_TFADTODAYTY,0) + nvl(TASK_TMCTODAYTY,0) + nvl(TASK_TFCTODAYTY,0)) *0.5) )))) ctodate,
				 sum(decode(sign(TASK_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),-1,0,decode(sign(TASK_DATE - to_date(:ra_ly_frdt,'dd/mm/yyyy')),1,0,(nvl(TASK_TMATODAYTY,0) + nvl(TASK_TFATODAYTY,0) + ((nvl(TASK_TMADTODAYTY,0) + nvl(TASK_TFADTODAYTY,0) + nvl(TASK_TMCTODAYTY,0) + nvl(TASK_TFCTODAYTY,0)) *0.5) )))) ltoday, 
