 retrieve="select a.EMP_ID,EMP_NAME, decode(EMP_TYPE,'SS','Sub Staff','ST','Staff','AT','Apperentice') EMP_TYPE,
           sum(decode(nvl(EATTEN_STATUS,'AB'),'WK',1,0)) Working,
           sum(decode(nvl(EATTEN_STATUS,'AB'),'WR',1,0)) rest,
           sum(decode(nvl(EATTEN_STATUS,'AB'),'HD',1,0)) holyday,
           sum(decode(nvl(EATTEN_STATUS,'AB'),'CL',1,0)) casual,       
           sum(decode(nvl(EATTEN_STATUS,'AB'),'SI',1,0) + decode(nvl(EATTEN_STATUS,'AB'),'SO',1,0)) Sick,
           sum(decode(nvl(EATTEN_STATUS,'AB'),'EL',1,0)) Earned,
           sum(decode(nvl(EATTEN_STATUS,'AB'),'SP',1,0)) Suspend,
           sum(decode(nvl(EATTEN_STATUS,'AB'),'MA',1,0)) Maternity,
		  sum(decode(nvl(EATTEN_STATUS,'AB'),'HW',1,0)) HolidaywPay,
              sum(decode(nvl(EATTEN_STATUS,'AB'),'AB',1,0))Absent
  from fb_empattendance a,fb_employee b
where a.emp_id= b.emp_id(+) and trunc(EATTEN_DATE) between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy') and
      	emp_type = decode(nvl(:ra_emp,'ALL'),'ALL',emp_type,:ra_emp)
group by a.emp_id,EMP_NAME, decode(EMP_TYPE,'SS','Sub Staff','ST','Staff','AT','Apperentice') " arguments=(("ra_frdt", string),("ra_todt", string),("ra_emp", string))  sort="emp_type A emp_id A " )
