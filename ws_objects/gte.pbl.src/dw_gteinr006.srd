 column=(type=datetime updatewhereclause=yes name=max_pur_issue dbname="max_pur_issue" )
 column=(type=number updatewhereclause=yes name=idel_days dbname="idel_days" )
 column=(type=number updatewhereclause=yes name=stock dbname="stock" )
