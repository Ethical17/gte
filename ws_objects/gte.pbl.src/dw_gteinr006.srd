 retrieve="select distinct initcap(decode(nvl(a.SSP_ID,'x'),'x',SP_NAME,e.SSP_NAME))||' ('||a.SP_ID||')' SP_NAME, A.SP_ID, a.SP_UNIT,invoice_date,issue_date,
           decode(sign(nvl(invoice_date,'01-mar-1999')-nvl(issue_date,'01-mar-1999')),1,invoice_date,0,invoice_date,issue_date) max_pur_issue,
		 trunc(sysdate) - decode(sign(nvl(invoice_date,'01-mar-1999')-nvl(issue_date,'01-mar-1999')),1,invoice_date,0,invoice_date,issue_date) idel_days,
	      stock,(stock_value/stock) rate, stock_value
   from fb_storeproduct a,
		  (select DS_ITEM_CD,sum(decode(ds_rec_ind,'I',-1,1)*ds_QTY) stock,sum(decode(ds_rec_ind,'I',-1,1)* nvl(ds_Value,0)) stock_value
		 	  from fb_daily_stock 
			group by DS_ITEM_CD)
			b,fb_storeproduct_std e,
           (select distinct SP_ID ,trunc(max(invoice_date)) invoice_date
	         from (select distinct b.SP_ID,trunc(max(HOPI_DATE)) invoice_date  from FB_HOPURCHASEINVOICE a,fb_hopidetails b  where a.HOPI_ID=b.HOPI_ID group by b.SP_ID
                       union
                       select distinct b.SP_ID, max(LPI_DATE)invoice_dt from fb_localpurchaseinvoice a,fb_lpidetails b  where a.LPI_ID=b.LPI_ID group by b.SP_ID)
	      group by SP_ID )bb,
          (select distinct b.SP_ID,max(trunc(PRIS_DATE))issue_date from fb_productissue a,fb_productissuedetails b  where a.PRIS_ID=b.PRIS_ID group by b.SP_ID) cc 
where a.SP_ID=bb.SP_ID(+) and a.sp_id=cc.sp_id(+) and a.SSP_ID=e.ssp_id(+) and (invoice_date is not null or issue_date is not null) and
		 a.SP_ID= b.DS_ITEM_CD and nvl(stock,0) > 0 and 
            a.SPC_ID=decode(:ra_spc_id,'ALL',a.SPC_ID,:ra_spc_id) and
            ((:ra_category = 'C' and (trunc(sysdate) - decode(sign(nvl(invoice_date,'01-mar-1999')-nvl(issue_date,'01-mar-1999')),1,invoice_date,0,invoice_date,issue_date)) > 365) or
	        (:ra_category = 'B' and (trunc(sysdate) - decode(sign(nvl(invoice_date,'01-mar-1999')-nvl(issue_date,'01-mar-1999')),1,invoice_date,0,invoice_date,issue_date)) between 181 and 365) or 
	        (:ra_category = 'A' and (trunc(sysdate) - decode(sign(nvl(invoice_date,'01-mar-1999')-nvl(issue_date,'01-mar-1999')),1,invoice_date,0,invoice_date,issue_date)) <= 180) or
              (:ra_category = 'ALL')) " arguments=(("ra_spc_id", string),("ra_category", string))  sort="item_catg D idel_days D sp_name A " )
