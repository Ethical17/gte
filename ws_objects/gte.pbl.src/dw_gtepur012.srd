 retrieve="select LPI_VOU_NO, LPI_VOU_DT,lpi.lpi_id, LPI_ISS_GSTNNO, LPI_REC_GSTNNO,LPI_REV_CHRG, LPI_REV_CAT,LPI_ISS_LOCN, LPI_REC_LOCN,  
    lpo.sup_id, initcap(sup_name) party_name,LPI_BILLNO, LPI_BILLDATE,RCI_INVOICE_NO,         
    LPI_QUANTITY,LPI_EFFECTIVEUNITPRICE, sum(nvl(LPI_EFFECTIVEUNITPRICE,0) * nvl(LPI_QUANTITY,0)) Amount, LPI_CGST_PER, LPI_SGST_PER, LPI_IGST_PER, 
    sum(nvl(LPI_CGST_AMT,0)) LPI_CGST_AMT, sum(nvl(LPI_SGST_AMT,0)) LPI_SGST_AMT, sum(nvl(LPI_IGST_AMT,0)) LPI_IGST_AMT,sp.SP_HSN_NO, sp.SP_UNIT
from fb_localpurchaseinvoice lpi, fb_lpidetails lpd, fb_localpurchaseorder lpo, fb_supplier sup, (select distinct RCI_INVOICE_NO, RCI_REF_NO from FB_REVCHARGE_INVOICE)  rc, fb_storeproduct sp
where lpi.lpi_id = lpd.lpi_id and lpi.lpo_id = lpo.lpo_id and lpo.sup_id = sup.sup_id(+) and lpi.lpi_vou_no = rc.rci_ref_no(+) and lpd.sp_id = sp.sp_id and 
       ((:ra_type = 'DT' and lpi_vou_dt between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy')) or   (:ra_type = 'YM' and to_char(lpi_vou_dt,'YYYYMM') = :ra_yrmn))
group by LPI_VOU_NO, LPI_VOU_DT,lpi.lpi_id, LPI_ISS_GSTNNO, LPI_REC_GSTNNO,LPI_REV_CHRG, LPI_REV_CAT,LPI_ISS_LOCN, LPI_REC_LOCN,  
    lpo.sup_id, initcap(sup_name),LPI_BILLNO, LPI_BILLDATE,RCI_INVOICE_NO,         
    LPI_QUANTITY,LPI_EFFECTIVEUNITPRICE,LPI_CGST_PER, LPI_SGST_PER, LPI_IGST_PER,sp.SP_HSN_NO, sp.SP_UNIT
union all
select vh_vou_no, vh_vou_date, null, SUP_GSTNNO, UNIT_GSTNNO, nvl(VD_REVCHG_IND, 'N'), VD_REVCHG_CATG, SUP_GSTN_STCD, UNIT_GSTN_STCD, sup_id, sup_name, VD_REF_NO, VD_REF_DATE, null, null, null,
VD_BILL_AMT, VD_CGST_PRCNT, VD_SGST_PRCNT, VD_IGST_PRCNT, VD_CGST_AMT, VD_SGST_AMT, VD_IGST_AMT, VD_HSN_CD, null
from fb_vou_head a, fb_vou_det b, fb_acsubledger c, fb_supplier d, fb_gardenmaster
where vh_doc_Srl = vd_Doc_srl  and vd_sgl_Cd = c.acsubledger_id and c.acsubledger_id = d.acsubledger_id  and nvl(unit_active_ind , 'N') = 'Y' and nvl(vd_gst_ind , 'N') = 'Y' and
 ((:ra_type = 'DT' and vh_vou_date between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy')) or   (:ra_type = 'YM' and to_char(vh_vou_date,'YYYYMM') = :ra_yrmn))
union all
select LPI_VOU_NO, LPI_VOU_DT,lpi.lpi_id, LPI_ISS_GSTNNO, LPI_REC_GSTNNO,LPI_REV_CHRG, LPI_REV_CAT,LPI_ISS_LOCN, LPI_REC_LOCN,  
    lpo.sup_id, initcap(sup_name) party_name,LPI_BILLNO, LPI_BILLDATE,RCI_INVOICE_NO,         
    LPI_QUANTITY,LPI_EFFECTIVEUNITPRICE, sum(nvl(LPI_EFFECTIVEUNITPRICE,0) * nvl(LPI_QUANTITY,0)) Amount, LPI_CGST_PER, LPI_SGST_PER, LPI_IGST_PER, 
    sum(nvl(LPI_CGST_AMT,0)) LPI_CGST_AMT, sum(nvl(LPI_SGST_AMT,0)) LPI_SGST_AMT, sum(nvl(LPI_IGST_AMT,0)) LPI_IGST_AMT,sp.SM_HSN_NO, 'OTH' SP_UNIT
from fb_servicepurchaseinvoice lpi, fb_spidetails lpd, fb_servicepurchaseorder lpo, fb_supplier sup, (select distinct RCI_INVOICE_NO, RCI_REF_NO from FB_REVCHARGE_INVOICE)  rc, fb_service_master sp
where lpi.lpi_id = lpd.lpi_id and lpi.lpo_id = lpo.lpo_id and lpo.sup_id = sup.sup_id(+) and lpi.lpi_vou_no = rc.rci_ref_no(+) and lpd.SM_ID = sp.SM_ID and 
       ((:ra_type = 'DT' and lpi_vou_dt between to_date(:ra_frdt,'dd/mm/yyyy') and to_date(:ra_todt,'dd/mm/yyyy')) or   (:ra_type = 'YM' and to_char(lpi_vou_dt,'YYYYMM') = :ra_yrmn))
group by LPI_VOU_NO, LPI_VOU_DT,lpi.lpi_id, LPI_ISS_GSTNNO, LPI_REC_GSTNNO,LPI_REV_CHRG, LPI_REV_CAT,LPI_ISS_LOCN, LPI_REC_LOCN,  
    lpo.sup_id, initcap(sup_name),LPI_BILLNO, LPI_BILLDATE,RCI_INVOICE_NO,         
    LPI_QUANTITY,LPI_EFFECTIVEUNITPRICE,LPI_CGST_PER, LPI_SGST_PER, LPI_IGST_PER,sp.SM_HSN_NO
