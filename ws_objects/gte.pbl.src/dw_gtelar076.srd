table(column=(type=char(1) updatewhereclause=yes name=prun_so dbname="prun_so" )
 column=(type=char(13) updatewhereclause=yes name=prun_type dbname="prun_type" )
 column=(type=char(30) updatewhereclause=yes name=div dbname="div" )
 column=(type=char(50) updatewhereclause=yes name=worker_name dbname="worker_name" )
 column=(type=char(25) updatewhereclause=yes name=worker_id dbname="worker_id" )
 column=(type=number updatewhereclause=yes name=grleaf_qty dbname="grleaf_qty" )
 column=(type=number updatewhereclause=yes name=div_avg dbname="div_avg" )
