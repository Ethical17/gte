 retrieve="select decode(Prun_type,'Pruned Leaf','A','C') prun_so, Prun_type, initcap(FIELD_NAME) div, null Worker_Name, null worker_id, 
       	sum(nvl(totalgl,0)) grleaf_qty, (sum(nvl(totalgl,0))/sum(nvl(Pluckingdays,0))) div_avg    ,(select initcap(UNIT_NAME) from fb_gardenmaster where UNIT_ACTIVE_IND='Y') garden
from (
		SELECT LABOUR_ID worker_id, initcap(EMP_NAME) Worker_Name, FIELD_NAME, decode(nvl(PRUN_STYLE,'x'),'x','Unpruned Leaf','Pruned Leaf' ) Prun_type,
				 sum(nvl(LDA_STATUS,0)) Pluckingdays,
				 sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) totalgl,
				 round(sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) / sum(nvl(LDA_STATUS,0)),2) Avg_div
		FROM   FB_EMPLOYEE a, FB_LABOURDAILYATTENDANCE b, 
				 (select * from FB_KAMSUBHEAD where trim(KAMSUB_TYPE) = 'PLUCK') c ,  
				 FB_LABOURSHEET f, fb_field g, (select * from FB_PRUNINGSESSION where PRUN_YEAR = 2022) h
		WHERE  b.KAMSUB_ID = c.KAMSUB_ID AND b.LABOUR_ID = a.emp_ID AND a.LS_ID = f.LS_ID AND f.FIELD_ID = g.FIELD_ID(+) and nvl(KAMSUB_ACTIVE_IND,'N') = 'Y' and
				   LDA_SECTIONID1 = h.SECTION_ID(+) and trunc(b.LDA_DATE) = to_date(:ra_frdt,'dd/mm/yyyy') and FIELD_NAME <> 'FACTORY'
		group by LABOUR_ID, initcap(EMP_NAME), FIELD_NAME, decode(nvl(PRUN_STYLE,'x'),'x','Unpruned Leaf','Pruned Leaf' )
		having sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) > 0
   	   )
group by decode(Prun_type,'Pruned Leaf','A','C'), Prun_type, FIELD_NAME
union 
select decode(Prun_type,'Pruned Leaf','B','D') prun_so, Prun_type, initcap(FIELD_NAME), Worker_Name, worker_id, 
       	Avg_div, round(100 - ((totalgl / sum_div_avg) * 100)) pluck_prcnt   ,(select initcap(UNIT_NAME) from fb_gardenmaster where UNIT_ACTIVE_IND='Y') garden
from (SELECT LABOUR_ID worker_id, initcap(EMP_NAME) Worker_Name, FIELD_NAME, decode(nvl(PRUN_STYLE,'x'),'x','Unpruned Leaf','Pruned Leaf' ) Prun_type,
               sum(nvl(LDA_STATUS,0)) Pluckingdays,
               sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) totalgl,
               round(sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) / sum(nvl(LDA_STATUS,0)),2) Avg_div
        FROM   FB_EMPLOYEE a, FB_LABOURDAILYATTENDANCE b, 
               (select * from FB_KAMSUBHEAD where trim(KAMSUB_TYPE) = 'PLUCK') c ,  
               FB_LABOURSHEET f, fb_field g, (select * from FB_PRUNINGSESSION where PRUN_YEAR = 2022) h
        WHERE  b.KAMSUB_ID = c.KAMSUB_ID AND b.LABOUR_ID = a.emp_ID AND a.LS_ID = f.LS_ID AND f.FIELD_ID = g.FIELD_ID(+) and nvl(KAMSUB_ACTIVE_IND,'N') = 'Y' and
               	 LDA_SECTIONID1 = h.SECTION_ID(+) and trunc(b.LDA_DATE) = to_date(:ra_frdt,'dd/mm/yyyy') and FIELD_NAME <> 'FACTORY'
        group by LABOUR_ID, initcap(EMP_NAME), FIELD_NAME, decode(nvl(PRUN_STYLE,'x'),'x','Unpruned Leaf','Pruned Leaf' )
        having sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) > 0),
      (select Prun_type sum_pty, FIELD_NAME sum_dty, sum(nvl(totalgl,0)) tot_pluck, 
              (sum(nvl(totalgl,0))/sum(nvl(Pluckingdays,0))) sum_div_avg, ((sum(nvl(totalgl,0))/sum(nvl(Pluckingdays,0))) * (1 - (:ra_prcnt/100))) div_avg_prcnt     
        from (
			  SELECT LABOUR_ID worker_id, initcap(EMP_NAME) Worker_Name, FIELD_NAME, decode(nvl(PRUN_STYLE,'x'),'x','Unpruned Leaf','Pruned Leaf' ) Prun_type,
						sum(nvl(LDA_STATUS,0)) Pluckingdays,
						sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) totalgl,
						round(sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) / sum(nvl(LDA_STATUS,0)),2) Avg_div
			  FROM   FB_EMPLOYEE a, FB_LABOURDAILYATTENDANCE b, 
						(select * from FB_KAMSUBHEAD where trim(KAMSUB_TYPE) = 'PLUCK') c ,  
						FB_LABOURSHEET f, fb_field g, (select * from FB_PRUNINGSESSION where PRUN_YEAR = 2022) h
			  WHERE  b.KAMSUB_ID = c.KAMSUB_ID AND b.LABOUR_ID = a.emp_ID AND a.LS_ID = f.LS_ID AND f.FIELD_ID = g.FIELD_ID(+) and nvl(KAMSUB_ACTIVE_IND,'N') = 'Y' and
						LDA_SECTIONID1 = h.SECTION_ID(+) and trunc(b.LDA_DATE) = to_date(:ra_frdt,'dd/mm/yyyy') and FIELD_NAME <> 'FACTORY' 
			  group by LABOUR_ID, initcap(EMP_NAME), FIELD_NAME, decode(nvl(PRUN_STYLE,'x'),'x','Unpruned Leaf','Pruned Leaf' )
			  having sum(nvl(LDA_MFJ_COUNT1,0) + nvl(LDA_MFJ_COUNT2,0) + nvl(LDA_MFJ_COUNT3,0)) > 0
            	 )
        group by decode(Prun_type,'Pruned Leaf','B','D'), Prun_type, FIELD_NAME)
where Prun_type = sum_pty and FIELD_NAME = sum_dty and Avg_div < div_avg_prcnt 
order by 1,2,3,4,5  

